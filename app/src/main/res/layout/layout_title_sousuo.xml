<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/A1">
    <!--a添加白名单标题-->
    <RelativeLayout
        android:id="@+id/normal_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d56">

        <FrameLayout
            android:id="@+id/title_left"
            android:layout_width="@dimen/d56"
            android:layout_height="@dimen/d56"
            android:layout_centerVertical="true"
            android:background="?attr/selectableItemBackground">

            <ImageView
                android:layout_width="@dimen/d24"
                android:layout_height="@dimen/d24"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/d16"
                android:src="@mipmap/tilte_left" />
        </FrameLayout>

        <com.myboost.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
            android:id="@+id/title_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/d72"
            android:text="@string/app_name"
            android:textColor="@color/A5"
            android:textSize="@dimen/s18" />
    </RelativeLayout>
</RelativeLayout>