<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A1"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="0dp" />

        <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/d70"
            android:gravity="center"
            android:text="@string/notifi_1"
            android:textColor="@color/A10"
            android:textSize="@dimen/s18" />

        <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/d10"
            android:gravity="center"
            android:text="@string/notifi_2"
            android:textColor="@color/A10"
            android:textSize="@dimen/s18" />

        <ImageView
            android:id="@+id/notifi_info_lot"
            android:layout_width="@dimen/d208"
            android:layout_height="@dimen/d346"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/d50"
            android:src="@mipmap/noti_phone" />

        <Button
            android:id="@+id/notifi_info_text"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="@dimen/d162"
            android:layout_height="@dimen/d34"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/d34"
            android:background="@drawable/main_clean_button"
            android:text="@string/gboost_k"
            android:textColor="@color/A1"
            android:textSize="@dimen/s18" />
    </LinearLayout>
</FrameLayout>