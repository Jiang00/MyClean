<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/manager_permision"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">


            <com.security.module.mView.CustomerTypefaceTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/premiss_2"
                android:textColor="@color/A9"
                android:textSize="@dimen/s12" />

            <com.security.module.mView.CustomerTypefaceTextView
                android:id="@+id/manager_shouquan"
                android:layout_width="@dimen/d190"
                android:layout_height="@dimen/d37"
                android:layout_marginTop="@dimen/d30"
                android:background="@drawable/shape__a4_r"
                android:gravity="center"
                android:text="@string/premiss_5"
                android:textColor="@color/white_100"
                android:textSize="@dimen/s15"
                app:typeface="Roboto-Medium.ttf" />
        </LinearLayout>

    </LinearLayout>
</FrameLayout>