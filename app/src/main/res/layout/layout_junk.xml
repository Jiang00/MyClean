<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A7"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/junk_title_backg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="0dp" />

        <include layout="@layout/layout_title" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/d146"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                    android:id="@+id/junk_size_all"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:gravity="center"
                    android:lines="1"
                    android:text="0"
                    android:textColor="@color/B3"
                    android:textSize="@dimen/s66"
                    app:typeface="ROBOTO-THIN_0.TTF" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d14"
                    android:layout_toRightOf="@+id/junk_size_all"
                    android:orientation="vertical">

                    <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                        android:id="@+id/junk_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="MB"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s16"
                        app:typeface="ROBOTO-LIGHT.TTF" />

                    <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                        android:id="@+id/junk_fangxin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d8"
                        android:text="@string/picture_tishi"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s14"
                        app:typeface="ROBOTO-LIGHT.TTF" />
                </LinearLayout>
            </LinearLayout>

            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                android:id="@+id/junk_button_clean2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d20"
                android:layout_alignParentBottom="true"
                android:background="#33ffffff"
                android:gravity="center"
                android:text="@string/junk_cleansize"
                android:textColor="@color/B3"
                android:textSize="@dimen/s12"
                app:typeface="ROBOTO-LIGHT.TTF" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/A7"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <ListView
                android:id="@+id/junk_list_all"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/B3"
                android:divider="@drawable/junk_divider"
                android:dividerHeight="1px"
                android:visibility="gone" />

            <com.privacy.junk.privacycustomview.ScrollView
                android:id="@+id/junk_scroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/B3"
                android:scrollbars="none">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/junk_button_system"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="?attr/selectableItemBackground"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d60"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="@dimen/d72"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="@dimen/d34"
                                    android:layout_height="@dimen/d34"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/junk_system" />
                            </LinearLayout>

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:id="@+id/customerTypefaceTextView2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical"
                                android:text="@string/junk_system"
                                android:textColor="@color/A6"
                                android:textSize="@dimen/s15" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:id="@+id/junk_system_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:text="10.0M"
                                android:textColor="@color/A9"
                                android:textSize="@dimen/s12" />

                            <ImageView
                                android:id="@+id/junk_system_jiantou"
                                android:layout_width="@dimen/d17"
                                android:layout_height="@dimen/d17"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/d25"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/junk_x"
                                android:visibility="visible" />
                        </LinearLayout>
                    </LinearLayout>

                    <com.privacy.junk.privacycustomview.ListViewForScrollViewPrivacy
                        android:id="@+id/junk_system_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/B3"
                        android:divider="@drawable/junk_divider"
                        android:dividerHeight="@dimen/d1" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d8"
                        android:background="@color/A7" />

                    <LinearLayout
                        android:id="@+id/junk_button_user"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="?attr/selectableItemBackground"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d60"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="@dimen/d72"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="@dimen/d34"
                                    android:layout_height="@dimen/d34"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/junk_user" />
                            </LinearLayout>

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical"
                                android:text="@string/junk_user"
                                android:textColor="@color/A6"
                                android:textSize="@dimen/s15" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:id="@+id/junk_user_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center"
                                android:text="10.0M"
                                android:textColor="@color/A9"
                                android:textSize="@dimen/s12" />

                            <ImageView
                                android:layout_width="@dimen/d17"
                                android:layout_height="@dimen/d17"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/d25"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/junk_x"
                                android:visibility="visible" />

                        </LinearLayout>
                    </LinearLayout>

                    <com.privacy.junk.privacycustomview.ListViewForScrollViewPrivacy
                        android:id="@+id/junk_user_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white_100"
                        android:divider="@drawable/junk_divider"
                        android:dividerHeight="@dimen/d1" />
                    <!--  android:dividerHeight="1px" 高度为1px的横线-->
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d8"
                        android:background="@color/A7" />

                    <LinearLayout
                        android:id="@+id/junk_button_log"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="?attr/selectableItemBackground"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d60"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="@dimen/d72"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="@dimen/d34"
                                    android:layout_height="@dimen/d34"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/junk_log" />
                            </LinearLayout>

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical"
                                android:text="@string/junk_log"
                                android:textColor="@color/A6"
                                android:textSize="@dimen/s15" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:id="@+id/junk_log_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center"
                                android:text="10.0M"
                                android:textColor="@color/A9"
                                android:textSize="@dimen/s12" />

                            <ImageView
                                android:id="@+id/junk_log_jiantou"
                                android:layout_width="@dimen/d17"
                                android:layout_height="@dimen/d17"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/d25"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/junk_x"
                                android:visibility="visible" />

                        </LinearLayout>
                    </LinearLayout>

                    <com.privacy.junk.privacycustomview.ListViewForScrollViewPrivacy
                        android:id="@+id/junk_log_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white_100"
                        android:divider="@drawable/junk_divider"
                        android:dividerHeight="@dimen/d1" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d8"
                        android:background="@color/A7" />

                    <LinearLayout
                        android:id="@+id/junk_button_apk"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="?attr/selectableItemBackground"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d60"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="@dimen/d72"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="@dimen/d34"
                                    android:layout_height="@dimen/d34"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/junk_apk" />
                            </LinearLayout>

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical"
                                android:text="@string/junk_apk"
                                android:textColor="@color/A6"
                                android:textSize="@dimen/s15" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:id="@+id/junk_apk_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center"
                                android:text="10.0M"
                                android:textColor="@color/A9"
                                android:textSize="@dimen/s12" />

                            <ImageView
                                android:id="@+id/junk_apk_jiaotou"
                                android:layout_width="@dimen/d17"
                                android:layout_height="@dimen/d17"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/d25"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/junk_x"
                                android:visibility="visible" />
                        </LinearLayout>
                    </LinearLayout>

                    <com.privacy.junk.privacycustomview.ListViewForScrollViewPrivacy
                        android:id="@+id/junk_apk_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white_100"
                        android:divider="@drawable/junk_divider"
                        android:dividerHeight="@dimen/d1" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d8"
                        android:background="@color/A7" />

                    <LinearLayout
                        android:id="@+id/junk_button_unload"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="?attr/selectableItemBackground"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d60"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="@dimen/d72"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="@dimen/d34"
                                    android:layout_height="@dimen/d34"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/junk_unload" />
                            </LinearLayout>

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical"
                                android:text="@string/junk_unload"
                                android:textColor="@color/A6"
                                android:textSize="@dimen/s15" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />

                            <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
                                android:id="@+id/junk_unload_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:gravity="center"
                                android:text="10.0M"
                                android:textColor="@color/A9"
                                android:textSize="@dimen/s12" />

                            <ImageView
                                android:id="@+id/junk_unload_jiantou"
                                android:layout_width="@dimen/d17"
                                android:layout_height="@dimen/d17"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/d25"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/junk_x"
                                android:visibility="visible" />
                        </LinearLayout>
                    </LinearLayout>

                    <com.privacy.junk.privacycustomview.ListViewForScrollViewPrivacy
                        android:id="@+id/junk_unload_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white_100"
                        android:divider="@drawable/junk_divider"
                        android:dividerHeight="@dimen/d1" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d8"
                        android:background="@color/A7" />
                </LinearLayout>
            </com.privacy.junk.privacycustomview.ScrollView>
        </FrameLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom">

        <TextView
            android:id="@+id/junk_button_clean1"
            android:layout_width="@dimen/d176"
            android:layout_height="@dimen/d42"
            android:layout_centerInParent="true"
            android:layout_marginBottom="@dimen/d27"
            android:background="@drawable/clean_button_yuan"
            android:gravity="center"
            android:text="@string/main_cleanup"
            android:textColor="@color/B3"
            android:textSize="@dimen/s17"
            android:visibility="invisible" />
    </RelativeLayout>
</LinearLayout>