<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A1"
    android:orientation="vertical">

    <View
        android:id="@+id/view_title_bar"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <ImageView
        android:layout_width="@dimen/d68"
        android:layout_height="@dimen/d68"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/d217"
        android:src="@mipmap/side_icon" />

    <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
        android:id="@+id/loading_tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/d22"
        android:text="@string/app_name"
        android:alpha="0"
        android:textColor="@color/B3"
        android:textSize="@dimen/s19" />

    <com.privacy.module.charge.saver.protectview.PrivacyCustomerTypefaceTextView
        android:id="@+id/loading_tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:alpha="0"
        android:layout_marginTop="@dimen/d9"
        android:text="@string/loading_describe"
        android:textColor="@color/B3"
        android:textSize="@dimen/s15" />
</LinearLayout>
