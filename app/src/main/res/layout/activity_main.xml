<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A10"
    android:orientation="vertical"
    tools:context="com.mutter.clean.junk.myActivity.MainActivity">


    <View
        android:id="@+id/view_title_bar"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/B7" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/title_hight"
                android:background="@color/B7">

                <ImageView
                    android:id="@+id/iv_title_left"
                    android:layout_width="@dimen/d40"
                    android:layout_height="@dimen/d40"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/d8"
                    android:layout_marginRight="@dimen/d8"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:clickable="true"
                    android:padding="@dimen/d8"
                    android:scaleType="center"
                    android:src="@mipmap/main_menu" />

                <ImageView
                    android:id="@+id/menu_hong"
                    android:layout_width="@dimen/d8"
                    android:layout_height="@dimen/d8"
                    android:layout_alignRight="@id/iv_title_left"
                    android:layout_marginTop="@dimen/d16"
                    android:src="@drawable/shape_a2" />

                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                    android:id="@+id/tv_title_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv_title_left"
                    android:text="@string/app_name"
                    android:textColor="@color/A8"
                    android:textSize="@dimen/s17"
                    app:typeface="lyp_text.otf" />

                <ImageView
                    android:id="@+id/iv_title_right"
                    android:layout_width="@dimen/d40"
                    android:layout_height="@dimen/d40"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/d8"
                    android:layout_marginRight="@dimen/d8"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="@dimen/d8"
                    android:scaleType="center"
                    android:src="@mipmap/main_setting" />


            </RelativeLayout>

            <com.mutter.clean.junk.myview.PullToRefreshLayout
                android:id="@+id/main_pull_refresh"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <include layout="@layout/layout_load_more" />

                <com.mutter.clean.junk.myview.MyScrollView
                    android:id="@+id/main_scroll_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:overScrollMode="never"
                    android:scrollbars="none">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/A10"
                        android:orientation="vertical">

                        <!--tou-->
                        <FrameLayout
                            android:id="@+id/main_scale_all"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d288"
                            android:layout_gravity="center"
                            android:background="@drawable/shape_main_circle">

                            <RelativeLayout
                                android:id="@+id/main_sd_air_button"
                                android:layout_width="@dimen/d176"
                                android:layout_height="@dimen/d176"
                                android:layout_marginLeft="@dimen/d33"
                                android:layout_marginTop="@dimen/d53">

                                <com.mutter.clean.junk.myview.RoundSd
                                    android:id="@+id/main_custom_sd"
                                    android:layout_width="@dimen/d176"
                                    android:layout_height="@dimen/d176"
                                    android:layout_centerInParent="true" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:text="@string/main_arc_sd"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s12"
                                        app:typeface="lyp_text.otf" />

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d14">

                                        <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                            android:id="@+id/main_sd_per"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:text="78"
                                            android:textColor="@color/A8"
                                            android:textSize="@dimen/s61"
                                            android:textStyle="bold"
                                            app:typeface="lyp_text.otf" />

                                        <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignBaseline="@id/main_sd_per"
                                            android:layout_marginTop="@dimen/d14"
                                            android:layout_toRightOf="@id/main_sd_per"
                                            android:text="%"
                                            android:textColor="@color/A8"
                                            android:textSize="@dimen/s30"
                                            android:textStyle="bold"
                                            app:typeface="lyp_text.otf" />
                                    </RelativeLayout>

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_sd_size"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginBottom="@dimen/d7"
                                        android:text="1.3/1.7GB"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s13"
                                        app:typeface="lyp_text.otf" />


                                </LinearLayout>
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/main_ram_air_button"
                                android:layout_width="@dimen/d125"
                                android:layout_height="@dimen/d125"
                                android:layout_gravity="right"
                                android:layout_marginRight="@dimen/d40"
                                android:layout_marginTop="@dimen/d8">

                                <com.mutter.clean.junk.myview.RoundRam
                                    android:id="@+id/main_custom_ram"
                                    android:layout_width="@dimen/d125"
                                    android:layout_height="@dimen/d125"
                                    android:layout_centerInParent="true" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:text="@string/main_arc_ram"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s11"
                                        app:typeface="lyp_text.otf" />

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/d9">

                                        <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                            android:id="@+id/main_ram_per"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:text="78"
                                            android:textColor="@color/A8"
                                            android:textSize="@dimen/s34"
                                            android:textStyle="bold"
                                            app:typeface="lyp_text.otf" />

                                        <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignBaseline="@id/main_ram_per"
                                            android:layout_centerHorizontal="true"
                                            android:layout_toRightOf="@id/main_ram_per"
                                            android:text="%"
                                            android:textColor="@color/A8"
                                            android:textSize="@dimen/s18"
                                            android:textStyle="bold"
                                            app:typeface="lyp_text.otf" />
                                    </RelativeLayout>


                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_ram_size"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/d5"
                                        android:text="1.3/1.7GB"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s11"
                                        app:typeface="lyp_text.otf" />


                                </LinearLayout>
                            </RelativeLayout>

                            <FrameLayout
                                android:id="@+id/ad_delete"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom|right"
                                android:layout_marginBottom="@dimen/d56"
                                android:layout_marginRight="@dimen/d67"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/main_circle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="@mipmap/air_backg"
                                    android:src="@mipmap/air_main" />

                                <FrameLayout
                                    android:id="@+id/main_ad"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@mipmap/air_backg"
                                    android:visibility="gone">

                                    <ImageView
                                        android:id="@+id/lot_family"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:src="@mipmap/air_ad" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="right"
                                        android:layout_marginRight="@dimen/d12"
                                        android:layout_marginTop="@dimen/d6"
                                        android:text="AD"
                                        android:textColor="@color/white_20"
                                        android:textSize="@dimen/d7" />

                                </FrameLayout>
                            </FrameLayout>

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:background="@mipmap/main_tou_backg" />
                        </FrameLayout>

                        <!--六个按钮-->
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d176"
                            android:layout_marginBottom="@dimen/d10"
                            android:layout_marginLeft="@dimen/d10"
                            android:layout_marginRight="@dimen/d10"
                            android:orientation="vertical">

                            <android.support.v4.view.ViewPager
                                android:id="@+id/viewpager"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" />
                        </FrameLayout>

                        <!--好评-->
                        <LinearLayout
                            android:id="@+id/main_rotate_all"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d10"
                            android:layout_marginLeft="@dimen/d10"
                            android:layout_marginRight="@dimen/d10"
                            android:background="@drawable/shape_ad"
                            android:elevation="@dimen/d2"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d106"
                                android:orientation="vertical">

                                <FrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/d16"
                                        android:text="@string/rotate_1"
                                        android:textColor="@color/A6"
                                        android:textSize="@dimen/s14" />

                                    <ImageView
                                        android:id="@+id/main_rotate_cha"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="right"
                                        android:layout_marginRight="@dimen/d3"
                                        android:layout_marginTop="@dimen/d3"
                                        android:src="@mipmap/rotate_cha" />
                                </FrameLayout>


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:layout_marginLeft="@dimen/d36"
                                        android:layout_marginRight="@dimen/d17"
                                        android:src="@mipmap/main_rotate_2" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:text="@string/rotate_2"
                                        android:textColor="@color/B3"
                                        android:textSize="@dimen/s16" />
                                </LinearLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/main_rotate_bad"
                                    android:layout_width="@dimen/d112"
                                    android:layout_height="@dimen/d29"
                                    android:background="@drawable/shape_rotate_bad"
                                    android:gravity="center"
                                    android:text="@string/main_rotate_bad"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s13" />

                                <TextView
                                    android:id="@+id/main_rotate_good"
                                    android:layout_width="@dimen/d112"
                                    android:layout_height="@dimen/d29"
                                    android:background="@drawable/shape_rotate_good"
                                    android:gravity="center"
                                    android:text="@string/main_rotate_good"
                                    android:textColor="@color/A8"
                                    android:textSize="@dimen/s13" />
                            </LinearLayout>

                        </LinearLayout>

                        <!--功能块-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d10"
                            android:layout_marginLeft="@dimen/d10"
                            android:layout_marginRight="@dimen/d10"
                            android:background="@drawable/shape_ad"
                            android:elevation="@dimen/d2"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/tuiguang_main"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical" />

                            <!--通知栏清理-->
                            <LinearLayout
                                android:id="@+id/main_notifi_button"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:background="?attr/selectableItemBackground"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/notifi_6" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_msg_notifi"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_notifi"
                                        android:textColor="@color/B2"
                                        android:textSize="@dimen/s14"
                                        app:typeface="lyp_text.otf" />

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d8"
                                        android:text="@string/notifi_msg_main"
                                        android:textColor="@color/B3"
                                        android:textSize="@dimen/s12"
                                        app:typeface="lyp_text.otf" />
                                </LinearLayout>
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/B1" />

                            <!--深度清理-->
                            <LinearLayout
                                android:id="@+id/main_power_button"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:background="?attr/selectableItemBackground"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/power_6" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_msg_power"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_power"
                                        android:textColor="@color/B2"
                                        android:textSize="@dimen/s14"
                                        app:typeface="lyp_text.otf" />

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d8"
                                        android:text="@string/power_3"
                                        android:textColor="@color/B3"
                                        android:textSize="@dimen/s12"
                                        app:typeface="lyp_text.otf" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/B1" />

                            <!--文件管理-->
                            <LinearLayout
                                android:id="@+id/main_file_button"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:background="?attr/selectableItemBackground"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/file_main" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_msg_file"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_file"
                                        android:textColor="@color/B2"
                                        android:textSize="@dimen/s14"
                                        app:typeface="lyp_text.otf" />

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d8"
                                        android:text="@string/file_main"
                                        android:textColor="@color/B3"
                                        android:textSize="@dimen/s12"
                                        app:typeface="lyp_text.otf" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/B1" />

                            <!--游戏加速-->
                            <LinearLayout
                                android:id="@+id/main_gboost_button"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:background="?attr/selectableItemBackground"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/gboost_main" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_msg_GBoost"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/gboost_0"
                                        android:textColor="@color/B2"
                                        android:textSize="@dimen/s14"
                                        app:typeface="lyp_text.otf" />

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d8"
                                        android:text="@string/gboost_10"
                                        android:textColor="@color/B3"
                                        android:textSize="@dimen/s12"
                                        app:typeface="lyp_text.otf" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/B1" />
                            <!--相似图片-->
                            <LinearLayout
                                android:id="@+id/main_picture_button"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:background="?attr/selectableItemBackground"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/picture_main" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:id="@+id/main_msg_picture"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_picture"
                                        android:textColor="@color/B2"
                                        android:textSize="@dimen/s14"
                                        app:typeface="lyp_text.otf" />

                                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d8"
                                        android:text="@string/picture_delete"
                                        android:textColor="@color/B3"
                                        android:textSize="@dimen/s12"
                                        app:typeface="lyp_text.otf" />
                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <!--广告-->
                        <LinearLayout
                            android:id="@+id/ad_native_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d10"
                            android:layout_marginLeft="@dimen/d10"
                            android:layout_marginRight="@dimen/d10"
                            android:background="@drawable/shape_ad"
                            android:elevation="@dimen/d2"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:visibility="gone" />
                    </LinearLayout>

                </com.mutter.clean.junk.myview.MyScrollView>

                <include layout="@layout/layout_load_more" />

            </com.mutter.clean.junk.myview.PullToRefreshLayout>
        </LinearLayout>


        <com.mingle.widget.LinearLayout
            android:id="@+id/ll_ad_full"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/A8"
