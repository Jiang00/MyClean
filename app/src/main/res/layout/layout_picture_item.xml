<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/d121"
    android:layout_height="@dimen/d121"
    android:layout_marginBottom="@dimen/d1"
    android:layout_marginLeft="@dimen/d1"
    android:layout_marginRight="@dimen/d1"
    android:layout_marginTop="@dimen/d1"
    android:orientation="vertical">

    <com.upupup.clean.junk.myview.RoundImageView
        android:id="@+id/picture_icon"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        app:roundHeight="@dimen/d1"
        app:roundWidth="@dimen/d1" />

    <ImageView
        android:id="@+id/picture_best"
        android:layout_width="@dimen/d32"
        android:layout_height="@dimen/d32"
        android:src="@mipmap/picture_xing" />

    <com.upupup.clean.junk.myview.RoundImageView
        android:id="@+id/picture_yinying"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black_35"
        android:scaleType="centerCrop"
        android:visibility="gone"
        app:roundHeight="@dimen/d1"
        app:roundWidth="@dimen/d1" />

    <ImageView
        android:id="@+id/picture_check"
        android:layout_width="@dimen/d30"
        android:layout_height="@dimen/d30"
        android:layout_gravity="right|bottom"
        android:padding="@dimen/d3"
        android:src="@mipmap/ram_normal" />

</FrameLayout>