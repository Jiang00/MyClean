<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/d3"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/picture_icon"
        android:layout_width="@dimen/d112"
        android:layout_height="@dimen/d112" />

    <ImageView
        android:id="@+id/picture_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:src="@mipmap/picture_normal" />

    <TextView
        android:id="@+id/picture_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="@dimen/d2"
        android:text="000"
        android:textColor="@color/white_100"
        android:textSize="@dimen/d12" />
</FrameLayout>