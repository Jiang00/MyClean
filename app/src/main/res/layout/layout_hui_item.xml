<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/d16"
    android:layout_marginRight="@dimen/d15"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="@dimen/d100"
        android:layout_height="@dimen/d141">

        <ImageView
            android:id="@+id/picture_icon"
            android:layout_width="@dimen/d100"
            android:layout_height="@dimen/d112"
            android:scaleType="centerCrop" />
        <!-- android:background="@drawable/shape_pic_hui_3"-->
        <ImageView
            android:id="@+id/picture_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/d3"
            android:layout_marginTop="@dimen/d3"
            android:scaleType="centerCrop"
            android:src="@mipmap/ram_normal" />

        <com.icleaner.module.charge.saver.protectview.CustomerTypefaceTextView
            android:id="@+id/picture_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:text="000"
            android:textColor="#000000"
            android:textSize="@dimen/s16" />
    </RelativeLayout>
</FrameLayout>