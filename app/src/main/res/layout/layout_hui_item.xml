<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/d5"
    android:orientation="vertical">

    <com.android.view.RoundImageView
        android:id="@+id/picture_icon"
        android:layout_width="@dimen/d109"
        android:layout_height="@dimen/d109" />

    <ImageView
        android:id="@+id/picture_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|bottom"
        android:padding="@dimen/d5"
        android:scaleType="centerCrop"
        android:src="@mipmap/ram_normal" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:src="@mipmap/hui_num" />

    <com.fast.module.charge.saver.view.CustomerTypefaceTextView
        android:id="@+id/picture_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="@dimen/d2"
        android:text="000"
        android:textColor="@color/white_100"
        android:textSize="@dimen/s12" />
</FrameLayout>