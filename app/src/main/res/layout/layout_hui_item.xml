<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/d6"
    android:layout_marginLeft="@dimen/d10"
    android:layout_marginRight="@dimen/d10"
    android:layout_marginTop="@dimen/d6"
    android:orientation="vertical">

    <com.mutter.clean.junk.myview.RoundImageView
        android:id="@+id/picture_icon"
        android:layout_width="@dimen/d90"
        android:layout_height="@dimen/d90" />

    <ImageView
        android:id="@+id/picture_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:paddingRight="@dimen/d3"
        android:paddingTop="@dimen/d3"
        android:src="@mipmap/ram_normal" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:src="@mipmap/hui_num" />

    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
        android:id="@+id/picture_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:text="000"
        android:textColor="@color/A8"
        android:textSize="@dimen/s11"
        app:typeface="lyp_text.otf" />
</FrameLayout>