<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/picture_icon"
        android:layout_width="@dimen/d146"
        android:layout_height="@dimen/d84"
        android:layout_margin="@dimen/d8" />

    <ImageView
        android:id="@+id/picture_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|bottom"
        android:paddingBottom="@dimen/d10"
        android:paddingRight="@dimen/d10"
        android:scaleType="centerCrop"
        android:src="@mipmap/ram_normal" />

    <ImageView
        android:layout_width="@dimen/d146"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="@dimen/d8"
        android:background="@mipmap/hui_num" />

    <com.vater.module.charge.saver.Mview.CustomerTypefaceTextView
        android:id="@+id/picture_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="@dimen/d10"
        android:text="000"
        android:textColor="@color/white_100"
        android:textSize="@dimen/s12" />
</FrameLayout>