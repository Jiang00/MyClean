                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d16"
                android:layout_marginTop="@dimen/d27"
                android:text="Sanxign"
                android:textColor="@color/A8"
                android:textSize="@dimen/s24"
                app:typeface="eos_clean.otf" />


            <com.my.module.charge.saver.view.CustomerTypefaceTextView
                android:id="@+id/message_android_version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Android"
                android:textColor="@color/A8"
                android:textSize="@dimen/s16"
                app:typeface="eos_clean.otf" />

        </LinearLayout>
    </LinearLayout>

