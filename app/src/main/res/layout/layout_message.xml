<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A10"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/junk_title_backg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/ad_fl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/d7"
        android:layout_marginRight="@dimen/d7"
        android:layout_marginTop="@dimen/d7"
        android:background="@drawable/shape_ad"
        android:elevation="@dimen/d2"
        android:visibility="gone">

        <LinearLayout
            android:id="@+id/ll_ad"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </FrameLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d7"
                android:layout_marginRight="@dimen/d7"
                android:layout_marginTop="@dimen/d7"
                android:background="@drawable/shape_ad"
                android:elevation="@dimen/d2"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/d33"
                    android:layout_marginLeft="@dimen/d8"
                    android:gravity="center_vertical"
                    android:text="@string/message_start"
                    android:textColor="@color/A5"
                    android:textSize="@dimen/s11" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_brand"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_brand"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_model"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_model"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_cpu"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_cpu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_ram"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_ram"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_sd"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_sd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_version"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_version"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_q_camera"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_q_camera"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_h_camera"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_h_camera"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:text="@string/message_fenbianlv"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s13"
                        app:typeface="ROBOTO-THIN_0.TTF" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/message_resolution"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/d8"

                        android:textColor="@color/B3"
                        android:textSize="@dimen/s11"
                        app:typeface="ROBOTO-THIN_0.TTF" />
                </RelativeLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/d7"
                android:background="@drawable/shape_ad"
                android:elevation="@dimen/d2"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.mutter.clean.junk.myview.MessageRoundView
                        android:id="@+id/message_r_sd"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d72"
                        android:layout_marginTop="@dimen/d10" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d10"
                        android:text="@string/main_arc_sd"
                        android:textColor="@color/A6"
                        android:textSize="@dimen/s11" />

                    <TextView
                        android:id="@+id/message_text_sd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d6"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s9" />

                    <TextView
                        android:id="@+id/message_button_sd"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d24"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginTop="@dimen/d6"
                        android:background="@drawable/shape_a7"
                        android:gravity="center"
                        android:text="@string/notification_button"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s12" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.mutter.clean.junk.myview.MessageRoundView
                        android:id="@+id/message_r_ram"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d72"
                        android:layout_marginTop="@dimen/d10" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d10"
                        android:text="@string/main_arc_ram"
                        android:textColor="@color/A6"
                        android:textSize="@dimen/s11" />

                    <TextView
                        android:id="@+id/message_text_ram"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d6"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s9" />

                    <TextView
                        android:id="@+id/message_button_ram"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d24"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginTop="@dimen/d6"
                        android:background="@drawable/shape_a7"
                        android:gravity="center"
                        android:text="@string/float_jiasu"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s12" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <com.mutter.clean.junk.myview.MessageRoundViewCpu
                        android:id="@+id/message_r_cpu"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d72"
                        android:layout_marginTop="@dimen/d10" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d10"
                        android:text="@string/main_arc_cpu"
                        android:textColor="@color/A6"
                        android:textSize="@dimen/s11" />

                    <TextView
                        android:id="@+id/message_text_cpu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d6"
                        android:text="Cool"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s9"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/message_button_cpu"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d24"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginTop="@dimen/d6"
                        android:background="@drawable/shape_a7"
                        android:gravity="center"
                        android:text="@string/notifi_jiangwen"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s12" />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</LinearLayout>