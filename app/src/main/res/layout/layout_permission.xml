<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/onclick"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <FrameLayout
        android:layout_width="@dimen/d333"
        android:layout_height="wrap_content"
        android:background="@color/A1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|bottom"
            android:gravity="center"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:src="@mipmap/rotate_cha" />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/d48"
                android:layout_marginLeft="@dimen/d20"
                android:layout_marginRight="@dimen/d20"
                android:background="@drawable/shape_ffffff_r"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/aaa"
                    android:layout_width="@dimen/d36"
                    android:layout_height="@dimen/d36"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/d10"
                    android:background="@mipmap/icon" />

                <com.security.module.mView.CustomerTypefaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/d8"
                    android:layout_marginRight="@dimen/d50"
                    android:layout_toRightOf="@id/aaa"
                    android:text="@string/app_name"
                    android:textColor="@color/Z1"
                    android:textSize="@dimen/s14" />

                <ImageView
                    android:layout_width="@dimen/d32"
                    android:layout_height="@dimen/d32"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"
                    android:src="@mipmap/side_check_passed" />
            </RelativeLayout>

            <com.security.module.mView.CustomerTypefaceTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="@dimen/d12"
                android:layout_marginLeft="@dimen/d16"
                android:layout_marginRight="@dimen/d16"
                android:layout_marginTop="@dimen/d14"
                android:gravity="center_horizontal"
                android:text="@string/permiss_7"
                android:textColor="@color/white_100"
                android:textSize="@dimen/s14" />

        </LinearLayout>


    </FrameLayout>


</LinearLayout>

