<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/onclick"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <FrameLayout
        android:layout_width="@dimen/d333"
        android:layout_height="wrap_content"
        android:background="@color/A1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|bottom"
            android:gravity="center"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/d65">

                <com.vector.module.mView.CustomerTypefaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_marginTop="@dimen/d24"
                    android:text="@string/permiss_shou"
                    android:textColor="@color/white_100"
                    android:textSize="@dimen/s15" />

                <ImageView
                    android:id="@+id/bt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:padding="@dimen/d16"
                    android:src="@mipmap/unload_delete" />
            </FrameLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/d78"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/d8"
                    android:layout_marginLeft="@dimen/d26"
                    android:layout_marginRight="@dimen/d26"
                    android:src="@drawable/shape_ffffff_r" />

                <ImageView
                    android:id="@+id/aaa"
                    android:layout_width="@dimen/d48"
                    android:layout_height="@dimen/d48"
                    android:layout_marginLeft="@dimen/d33"
                    android:layout_marginTop="@dimen/d9"
                    android:background="@mipmap/icon" />

                <com.vector.module.mView.CustomerTypefaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/d12"
                    android:layout_marginTop="@dimen/d29"
                    android:layout_toRightOf="@id/aaa"
                    android:fontFamily="sans-serif"
                    android:text="@string/app_name"
                    android:textColor="@color/A8"
                    android:textSize="@dimen/s15" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/d17"
                    android:background="@mipmap/permiss_2" />

            </RelativeLayout>


        </LinearLayout>


    </FrameLayout>


</LinearLayout>

