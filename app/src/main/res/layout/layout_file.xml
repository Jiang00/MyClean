<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_100"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="24dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/A5"
            android:orientation="vertical">


            <LinearLayout
                android:id="@+id/file_apk_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d86"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100">

                <LinearLayout
                    android:layout_width="@dimen/d72"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_apk_button" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/file_apk"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_apk_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_apk_num_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/d5"
                        android:text="@string/file_num"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s14"
                        android:visibility="invisible" />
                </LinearLayout>


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/d8"
                    android:rotation="-90"
                    android:src="@mipmap/junk_right" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/file_zip_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d86"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100">

                <LinearLayout
                    android:layout_width="@dimen/d72"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_zip_button" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/file_zip"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_zip_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_zip_num_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:text="@string/file_num"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s14"
                        android:visibility="invisible" />
                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/d8"
                    android:rotation="-90"
                    android:src="@mipmap/junk_right" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/file_txt_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d86"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100">

                <LinearLayout
                    android:layout_width="@dimen/d72"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_txt_button" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/file_txt"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_txt_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_txt_num_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:text="@string/file_num"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s14"
                        android:visibility="invisible" />
                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/d8"
                    android:rotation="-90"
                    android:src="@mipmap/junk_right" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/file_music_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d86"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100">

                <LinearLayout
                    android:layout_width="@dimen/d72"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_music_button" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/file_music"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_music_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_music_num_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:text="@string/file_num"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s14"
                        android:visibility="invisible" />
                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/d8"
                    android:rotation="-90"
                    android:src="@mipmap/junk_right" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/file_video_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d86"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100">

                <LinearLayout
                    android:layout_width="@dimen/d72"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_video_button" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/file_video"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_video_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_video_num_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:text="@string/file_num"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s14"
                        android:visibility="invisible" />
                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/d8"
                    android:rotation="-90"
                    android:src="@mipmap/junk_right" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/file_other_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d86"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100">

                <LinearLayout
                    android:layout_width="@dimen/d72"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_other_button" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/file_pther"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/d24"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="vertical">


                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_qita_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/A11"
                        android:textSize="@dimen/s14" />

                    <com.vector.module.mView.CustomerTypefaceTextView
                        android:id="@+id/file_qita_num_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:text="@string/file_num"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s14"
                        android:visibility="invisible" />
                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/d8"
                    android:rotation="-90"
                    android:src="@mipmap/junk_right" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_ad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100"
                android:orientation="vertical"
                android:visibility="gone" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>