                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d4"
                        android:text="0B"
                        android:textColor="#818181"
                        android:textSize="@dimen/s11"
                        app:typeface="eos_clean.otf" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:src="@mipmap/file_other" />

                    <com.my.module.charge.saver.view.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d4"
                        android:text="@string/file_pther"
                        android:textColor="#818181"
                        android:textSize="@dimen/s11"
                        app:typeface="eos_clean.otf" />

