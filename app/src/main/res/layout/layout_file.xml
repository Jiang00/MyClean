<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_100"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="24dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/B2"
        android:overScrollMode="never"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/d121"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@drawable/shape_ffffff_10"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/file_sd_keyong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="20GB"
                    android:textColor="@color/Z1"
                    android:textSize="@dimen/s16" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/d8"
                    android:layout_marginRight="@dimen/d8"
                    android:layout_marginTop="@dimen/d20"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/file_title_video" />

                        <TextView
                            android:id="@+id/file_video_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d8"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/file_title_apk" />

                        <TextView
                            android:id="@+id/file_apk_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d8"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/file_title_audio" />

                        <TextView
                            android:id="@+id/file_audio_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d8"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/file_title_zip" />

                        <TextView
                            android:id="@+id/file_zip_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d8"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/file_title_dec" />

                        <TextView
                            android:id="@+id/file_dec_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d8"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/file_video_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d135"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@drawable/shape_ffffff_10">

                <LinearLayout
                    android:layout_width="@dimen/d78"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_video_button" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/d8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d48">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="@string/file_video"
                            android:textColor="@color/Z1"
                            android:textSize="@dimen/s16" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/file_video_size_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/B3" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:orientation="vertical">

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_video_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/Z1"
                                android:textSize="@dimen/s16" />

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_video_num_d"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/d14"
                                android:text="@string/file_main"
                                android:textColor="@color/Z2"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/file_video_num_check"
                            android:layout_width="@dimen/d53"
                            android:layout_height="@dimen/d24"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_a2_round"
                            android:gravity="center"
                            android:text="@string/gboost_9"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/file_music_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d135"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@drawable/shape_ffffff_10">

                <LinearLayout
                    android:layout_width="@dimen/d78"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_music_button" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/d8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d48">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="@string/file_music"
                            android:textColor="@color/Z1"
                            android:textSize="@dimen/s16" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/file_music_size_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/B3" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:orientation="vertical">

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_music_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/Z1"
                                android:textSize="@dimen/s16" />

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_music_num_d"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/d14"
                                android:text="@string/file_main"
                                android:textColor="@color/Z2"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/file_music_num_check"
                            android:layout_width="@dimen/d53"
                            android:layout_height="@dimen/d24"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_a2_round"
                            android:gravity="center"
                            android:text="@string/gboost_9"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/file_apk_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d135"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@drawable/shape_ffffff_10">

                <LinearLayout
                    android:layout_width="@dimen/d78"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_apk_button" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/d8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d48">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="@string/file_apk"
                            android:textColor="@color/Z1"
                            android:textSize="@dimen/s16" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/file_apk_size_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/B3" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:orientation="vertical">

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_apk_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/Z1"
                                android:textSize="@dimen/s16" />

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_apk_num_d"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/d14"
                                android:text="@string/file_main"
                                android:textColor="@color/Z2"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/file_apk_num_check"
                            android:layout_width="@dimen/d53"
                            android:layout_height="@dimen/d24"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_a2_round"
                            android:gravity="center"
                            android:text="@string/gboost_9"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/file_zip_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d135"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@drawable/shape_ffffff_10">

                <LinearLayout
                    android:layout_width="@dimen/d78"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_zip_button" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/d8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d48">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="@string/file_zip"
                            android:textColor="@color/Z1"
                            android:textSize="@dimen/s16" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/file_zip_size_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/B3" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:orientation="vertical">

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_zip_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/Z1"
                                android:textSize="@dimen/s16" />

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_zip_num_d"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/d14"
                                android:text="@string/file_main"
                                android:textColor="@color/Z2"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/file_zip_num_check"
                            android:layout_width="@dimen/d53"
                            android:layout_height="@dimen/d24"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_a2_round"
                            android:gravity="center"
                            android:text="@string/gboost_9"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/file_txt_button"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d135"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@drawable/shape_ffffff_10">

                <LinearLayout
                    android:layout_width="@dimen/d78"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_txt_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/d8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d48">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="@string/file_txt"
                            android:textColor="@color/Z1"
                            android:textSize="@dimen/s16" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/file_txt_size_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/Z2"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/B3" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:orientation="vertical">

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_txt_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/Z1"
                                android:textSize="@dimen/s16" />

                            <com.security.module.mView.CustomerTypefaceTextView
                                android:id="@+id/file_txt_num_d"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/d14"
                                android:text="@string/file_main"
                                android:textColor="@color/Z2"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/file_txt_num_check"
                            android:layout_width="@dimen/d53"
                            android:layout_height="@dimen/d24"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_a2_round"
                            android:gravity="center"
                            android:text="@string/gboost_9"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s12" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_ad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d8"
                android:layout_marginRight="@dimen/d8"
                android:layout_marginTop="@dimen/d8"
                android:background="@color/white_100"
                android:orientation="vertical"
                android:visibility="gone" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>