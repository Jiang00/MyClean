<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_100"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="24dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <!--六个按钮-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/file_apk_button"
            android:layout_width="match_parent"
            android:layout_height="@dimen/d72"
            android:orientation="horizontal">

            <FrameLayout
                android:layout_width="@dimen/d72"
                android:layout_height="@dimen/d72">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@mipmap/file_apk_button" />
            </FrameLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/file_apk"
                    android:textColor="#363636"
                    android:textSize="@dimen/s16"
                    app:typeface="ljh_1.TTF" />

                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                    android:id="@+id/file_apk_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/d4"
                    android:text="@string/file_xuyao"
                    android:textColor="#ed5565"
                    android:textSize="@dimen/s10"
                    android:visibility="gone"
                    app:typeface="ljh_1.TTF" />
            </LinearLayout>

            <FrameLayout
                android:layout_width="@dimen/d72"
                android:layout_height="match_parent">

                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                    android:id="@+id/apk_chakan"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/d30"
                    android:layout_gravity="center"
                    android:background="@drawable/shape_a2_2"
                    android:gravity="center"
                    android:paddingLeft="@dimen/d15"
                    android:paddingRight="@dimen/d15"
                    android:text="@string/file_chakan"
                    android:textColor="@color/white_100"
                    android:textSize="@dimen/s13"
                    android:visibility="invisible"
                    app:typeface="ljh_1.TTF" />

                <LinearLayout
                    android:id="@+id/apk_null"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="invisible">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/file_null" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d5"
                        android:text="@string/file_null"
                        android:textColor="#31cf62"
                        android:textSize="@dimen/s11" />
                </LinearLayout>
            </FrameLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#e5e5e5" />
    </LinearLayout>

