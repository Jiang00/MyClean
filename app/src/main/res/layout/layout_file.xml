                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/file_sd" />

                <com.my.module.charge.saver.view.CustomerTypefaceTextView
                    android:id="@+id/file_sd_shengyu"
