<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/success"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A1"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:background="@mipmap/clean_beijing" />

        <com.eifmobi.clean.junk.myview.BubbleLineLayout
            android:id="@+id/clean_bubble"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:rotation="60" />

        <ImageView
            android:id="@+id/clean_huojian"
            android:layout_width="@dimen/d318"
            android:layout_height="@dimen/d233"
            android:layout_gravity="center_vertical"
            android:src="@mipmap/ani_huojian" />

        <TextView
            android:id="@+id/success_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="@dimen/d33"
            android:text="@string/clean_success"
            android:textColor="@color/white_100"
            android:textSize="@dimen/s18" />


        <com.eifmobi.clean.junk.myview.SlowScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:scrollbars="none"
            android:visibility="invisible">
            <!--android:visibility="invisible"-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/A1"
                android:orientation="vertical">
                <!--tou-->
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d100">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/d29"
                            android:layout_height="@dimen/d29"
                            android:src="@mipmap/success_duigou" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/d18"
                            android:orientation="vertical">

                            <com.eifmobi.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/success_clean_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/success_cleaned"
                                android:textColor="@color/white_100"
                                android:textSize="@dimen/s17" />
                        </LinearLayout>
                    </LinearLayout>


                </FrameLayout>

                <FrameLayout
                    android:id="@+id/ad_fl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d8"
                    android:layout_marginLeft="@dimen/d16"
                    android:layout_marginRight="@dimen/d16"
                    android:background="@drawable/shape_a3"
                    android:visibility="gone">
                    <!--ad-->
                    <LinearLayout
                        android:id="@+id/ll_ad_xiao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/d6"
                        android:orientation="vertical" />
                </FrameLayout>
                <!--好评-->
                <LinearLayout
                    android:id="@+id/main_rotate_all"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d8"
                    android:layout_marginLeft="@dimen/d16"
                    android:layout_marginRight="@dimen/d16"
                    android:background="@drawable/shape_a3"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/main_rotate_cha"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:padding="@dimen/d8"
                        android:src="@mipmap/rotate_bai_cha" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/d24"
                        android:layout_marginRight="@dimen/d24"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/rotate_2" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/d16"
                            android:text="@string/rotate_2"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s14" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="@dimen/d16"
                        android:layout_marginTop="@dimen/d16"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/main_rotate_bad"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d12"
                            android:gravity="center"
                            android:padding="@dimen/d5"
                            android:text="@string/main_rotate_bad"
                            android:textColor="@color/white_30"
                            android:textSize="@dimen/s14" />

                        <TextView
                            android:id="@+id/main_rotate_good"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d20"
                            android:gravity="center"
                            android:padding="@dimen/d5"
                            android:text="@string/main_rotate_good"
                            android:textColor="@color/A14"
                            android:textSize="@dimen/s14" />
                    </LinearLayout>

                </LinearLayout>
                <!--功能块-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <!--垃圾清理-->
                    <LinearLayout
                        android:id="@+id/main_junk_button"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d56"
                        android:layout_marginBottom="@dimen/d8"
                        android:layout_marginLeft="@dimen/d16"
                        android:layout_marginRight="@dimen/d16"
                        android:background="@drawable/shape_a3"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d56"
                            android:background="?attr/selectableItemBackground">

                            <com.eifmobi.charge.saver.Aview.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/d16"
                                android:text="@string/side_junk"
                                android:textColor="@color/white_100"
                                android:textSize="@dimen/s16" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/success_right" />
                        </RelativeLayout>

                    </LinearLayout>

                    <!--内存加速-->
                    <LinearLayout
                        android:id="@+id/main_ram_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d8"
                        android:layout_marginLeft="@dimen/d16"
                        android:layout_marginRight="@dimen/d16"
                        android:background="@drawable/shape_a3"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d56"
                            android:background="?attr/selectableItemBackground">

                            <com.eifmobi.charge.saver.Aview.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/d16"
                                android:text="@string/side_ram"
                                android:textColor="@color/white_100"
                                android:textSize="@dimen/s16" />


                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/success_right" />


                        </RelativeLayout>

                    </LinearLayout>

                    <!--手机降温-->
                    <LinearLayout
                        android:id="@+id/main_cooling_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d8"
                        android:layout_marginLeft="@dimen/d16"
                        android:layout_marginRight="@dimen/d16"
                        android:background="@drawable/shape_a3"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d56"
                            android:background="?attr/selectableItemBackground">

                            <com.eifmobi.charge.saver.Aview.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/d16"
                                android:text="@string/main_cooling_name"
                                android:textColor="@color/white_100"
                                android:textSize="@dimen/s16" />


                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="@dimen/d16"
                                android:src="@mipmap/success_right" />


                        </RelativeLayout>

                    </LinearLayout>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ad_native_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d8"
                    android:layout_marginLeft="@dimen/d16"
                    android:layout_marginRight="@dimen/d16"
                    android:background="@drawable/shape_a3"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:visibility="gone" />


            </LinearLayout>
        </com.eifmobi.clean.junk.myview.SlowScrollView>
    </FrameLayout>
</LinearLayout>