<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent">

    <LinearLayout
        android:id="@+id/success"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/A1"
        android:orientation="vertical">
        <!--标题-->
        <LinearLayout
            android:id="@+id/rl_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:id="@+id/view_title_bar"
                android:layout_width="match_parent"
                android:layout_height="0dp" />

            <include layout="@layout/layout_title" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/gradient_beijing">

            <FrameLayout
                android:id="@+id/success_animator"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="top|center_horizontal"
                android:visibility="gone">

                <!--画圆形进度条-->
                <com.myboost.junk.customviewboost.MainRoundViewBoost
                    android:id="@+id/main_progress"
                    android:layout_width="@dimen/d154"
                    android:layout_height="@dimen/d154"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/d73"
                    android:visibility="visible" />

                <!--背景-->
                <ImageView
                    android:layout_width="@dimen/d180"
                    android:layout_height="@dimen/d180"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/d60"
                    android:src="@mipmap/success_1"
                    android:visibility="visible" />
                <!--左眼睛-->
                <ImageView
                    android:id="@+id/success_eye_leght"
                    android:layout_width="@dimen/d180"
                    android:layout_height="@dimen/d180"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/d60"
                    android:alpha="0"
                    android:src="@mipmap/success_2"
                    android:visibility="visible" />
                <!--右眼睛-->
                <ImageView
                    android:id="@+id/success_eye_right1"
                    android:layout_width="@dimen/d180"
                    android:layout_height="@dimen/d180"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/d60"
                    android:alpha="0"
                    android:src="@mipmap/success_5"
                    android:visibility="visible" />
                <!--右眼睛，笑-->
                <ImageView
                    android:id="@+id/success_eye_right"
                    android:layout_width="@dimen/d180"
                    android:layout_height="@dimen/d180"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/d60"
                    android:alpha="0"
                    android:src="@mipmap/success_6"
                    android:visibility="visible" />
                <!--慢慢显示，嘴巴-->
                <com.myboost.junk.customviewboost.RenderingView
                    android:id="@+id/success_mouth"
                    android:layout_width="@dimen/d180"
                    android:layout_height="@dimen/d180"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/d60"
                    android:visibility="gone" />
            </FrameLayout>

            <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                android:id="@+id/success_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|center_horizontal"
                android:layout_marginTop="@dimen/d270"
                android:text="@string/success"
                android:textColor="@color/A5"
                android:textSize="@dimen/s24"
                android:visibility="visible" />

            <com.myboost.junk.customviewboost.BoostSlowScrollView
                android:id="@+id/scrollView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/A11"
                android:fillViewport="true"
                android:scrollbars="none"
                android:visibility="invisible">
                <!--android:visibility="invisible"-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <!--tou-->
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/B3">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d120"
                            android:background="@color/A1"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="@dimen/d158"
                                    android:layout_height="@dimen/d112"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginRight="@dimen/d8"
                                    android:src="@mipmap/success_duigou" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:id="@+id/success_complete"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d26"
                                    android:text="@string/success"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s32" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:id="@+id/success_clean_size"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/success_complete"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:text="@string/success_cleaned"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s16" />
                            </RelativeLayout>

                        </FrameLayout>
                    </FrameLayout>

                    <!--好评-->
                    <LinearLayout
                        android:id="@+id/main_rotate_all"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:layout_marginTop="@dimen/d4"
                        android:background="@color/B3"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d90"
                                android:background="@mipmap/praise" />

                            <ImageView
                                android:id="@+id/main_rotate_close"
                                android:layout_width="@dimen/d13"
                                android:layout_height="@dimen/d13"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="@dimen/d8"
                                android:layout_marginTop="@dimen/d10"
                                android:src="@mipmap/close1" />
                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d32"
                            android:layout_marginTop="@dimen/d20"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/mian_like_tishi1"
                                android:textColor="@color/A3"
                                android:textSize="@dimen/s16" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/A11" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d42"
                            android:layout_marginBottom="@dimen/d8"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">

                            <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                android:id="@+id/main_rotate_bad"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/d42"
                                android:layout_gravity="center_horizontal"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/rotate_no"
                                android:textColor="#80000000"
                                android:textSize="@dimen/s16" />

                            <TextView
                                android:layout_width="1px"
                                android:layout_height="@dimen/d29"
                                android:layout_gravity="center_vertical"
                                android:background="@color/A11" />

                            <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                android:id="@+id/main_rotate_good"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/d42"
                                android:layout_gravity="center_horizontal"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/rotate_ok"
                                android:textColor="@color/A1"
                                android:textSize="@dimen/s16" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d4"
                            android:background="@color/A11" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_ad_xiao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d4"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:layout_marginTop="@dimen/d4"
                        android:background="@color/B3"
                        android:orientation="vertical"
                        android:visibility="gone" />

                    <!--功能块-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d8"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:layout_marginTop="@dimen/d4"
                        android:background="@color/A11"
                        android:orientation="vertical">

                        <!--温度-->
                        <RelativeLayout
                            android:id="@+id/main_cooling_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground"
                            android:gravity="center_vertical">

                            <LinearLayout
                                android:id="@+id/main_cooling_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/cooling_button" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@+id/main_cooling_iv"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/main_cooling_name"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:lines="1"
                                    android:text="@string/main_cooltishi"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>
                        </RelativeLayout>

                        <!--通知栏-->
                        <RelativeLayout
                            android:id="@+id/main_notifi_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground"
                            android:gravity="center_vertical">

                            <LinearLayout
                                android:id="@+id/main_notifi_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/notifi_6" />
                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@+id/main_notifi_iv"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:id="@+id/main_msg_cooling"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_notifi"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:lines="1"
                                    android:text="@string/main_notify"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>
                        </RelativeLayout>

                        <!--相似图片-->
                        <RelativeLayout
                            android:id="@+id/main_picture_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground">

                            <LinearLayout
                                android:id="@+id/main_picture_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/picture_main" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@+id/main_picture_iv"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:id="@+id/main_msg_picture"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_picture"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:lines="1"
                                    android:text="@string/picture_delete"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>

                        </RelativeLayout>
                        <!--深度清理-->
                        <RelativeLayout
                            android:id="@+id/main_power_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground">

                            <LinearLayout
                                android:id="@+id/main_power_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/power_6" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@+id/main_power_iv"
                                android:layout_weight="1"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:id="@+id/main_msg_power"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_power"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:lines="1"
                                    android:text="@string/power_3"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>

                        </RelativeLayout>
                        <!--游戏加速-->
                        <RelativeLayout
                            android:id="@+id/main_gboost_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground">

                            <LinearLayout
                                android:id="@+id/main_gboost_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/gboost_button" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@+id/main_gboost_iv"
                                android:layout_weight="1"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/gboost_0"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:text="@string/gboost_10"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>
                        </RelativeLayout>
                        <!--文件-->
                        <RelativeLayout
                            android:id="@+id/main_file_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground">

                            <LinearLayout
                                android:id="@+id/main_file_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/file_main" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@id/main_file_iv"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_file"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:text="@string/file_main"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>

                        </RelativeLayout>

                        <!--垃圾清理-->
                        <RelativeLayout
                            android:id="@+id/main_junk_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8">

                            <LinearLayout
                                android:id="@+id/main_junk_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/junk_button" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@id/main_junk_iv"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_junk"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:text="@string/success_8"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>
                        </RelativeLayout>

                        <!--内存加速-->
                        <RelativeLayout
                            android:id="@+id/main_ram_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:layout_marginBottom="@dimen/d8"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/main_ram_iv"
                                android:layout_width="@dimen/d64"
                                android:layout_height="match_parent"
                                android:background="@color/A1"
                                android:gravity="center">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:src="@mipmap/ram_button" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toRightOf="@id/main_ram_iv"
                                android:background="@color/A5"
                                android:orientation="vertical">

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_ram"
                                    android:textColor="@color/A2"
                                    android:textSize="@dimen/s16" />

                                <com.myboost.module.charge.saver.protectview.CustomerTypefaceTextViewBoost
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:layout_marginTop="@dimen/d1"
                                    android:text="@string/success_9"
                                    android:textColor="@color/A4"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <!--大屏native-->
                    <LinearLayout
                        android:id="@+id/ad_native_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d8"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:background="@drawable/shape_ad"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
