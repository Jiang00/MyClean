                                android:layout_marginLeft="@dimen/d16"
                                android:src="@drawable/shape_lv_r" />

                            <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d9"
                                android:text="@string/main_rotate_title"
                                android:textColor="#414141"
                                android:textSize="@dimen/s15"
                                app:typeface="ljh_1.TTF" />
                        </LinearLayout>


                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d82"
                            android:layout_marginBottom="@dimen/d16"
                            android:scaleType="centerCrop"
                            android:src="@mipmap/main_rotate_2" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:text="@string/rotate_msg"
                            android:textColor="#5d5d5d"
                            android:textSize="@dimen/s15" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/d4"
                            android:text="@string/rotate_msg_2"
                            android:textColor="#8a5d5d5d"
                            android:textSize="@dimen/s14" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d50"
                            android:layout_marginTop="@dimen/d10"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:id="@+id/main_rotate_bad"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:text="@string/main_rotate_bad"
                                    android:textColor="#949494"
                                    android:textSize="@dimen/s12" />
                            </FrameLayout>

                            <FrameLayout
                                android:id="@+id/main_rotate_good"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <TextView
                                    android:layout_width="@dimen/d146"
                                    android:layout_height="@dimen/d36"
                                    android:layout_gravity="center"
                                    android:background="@drawable/shape_2fb5ff"
                                    android:gravity="center"
                                    android:text="@string/main_rotate_good"
                                    android:textColor="@color/white_100"
                                    android:textSize="@dimen/s16" />

                            </FrameLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d9"
                            android:background="#f5f7fa" />
                    </LinearLayout>

                    <LinearLayout
