<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/success"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A1"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d33">

            <com.sample.lottie.LottieAnimationView
                android:id="@+id/lottie_power"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:lottie_fileName="success.json"
                app:lottie_loop="false" />

            <LinearLayout
                android:id="@+id/success_ani_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:visibility="invisible">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/success_title_1"
                    android:textColor="@color/A8"
                    android:textSize="@dimen/s20" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/d6"
                    android:text="@string/success_title_2"
                    android:textColor="@color/A8"
                    android:textSize="@dimen/s16" />

            </LinearLayout>

        </FrameLayout>

        <com.upupup.clean.junk.myview.SlowScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/A1"
            android:fillViewport="true"
            android:scrollbars="none"
            android:visibility="invisible">
            <!--android:visibility="invisible"-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <!--tou-->

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d82"
                    android:layout_marginBottom="@dimen/d10">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/success_clean_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/success_title_1"
                                android:textColor="@color/A8"
                                android:textSize="@dimen/s20"
                                 />

                            <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/success_clean_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d8"
                                android:text="@string/success_title_2"
                                android:textColor="@color/A8"
                                android:textSize="@dimen/s16"
                                 />
                        </LinearLayout>
                    </LinearLayout>


                </FrameLayout>

                <!--好评-->
                <LinearLayout
                    android:id="@+id/main_rotate_all"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d106"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/d14"
                                android:layout_marginTop="@dimen/d14"
                                android:text="@string/rotate_1"
                                android:textColor="@color/black_87"
                                android:textSize="@dimen/s16" />

                            <ImageView
                                android:id="@+id/main_rotate_cha"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:layout_marginRight="@dimen/d3"
                                android:layout_marginTop="@dimen/d3"
                                android:padding="@dimen/d10"
                                android:src="@mipmap/rotate_cha" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/d14"
                            android:layout_marginTop="@dimen/d8"
                            android:text="@string/rotate_2"
                            android:textColor="@color/black_54"
                            android:textSize="@dimen/s13" />

                        <com.sample.lottie.LottieAnimationView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/d5"
                            app:lottie_autoPlay="true"
                            app:lottie_fileName="rate_stars.json"
                            app:lottie_loop="true" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d6"
                        android:gravity="right"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/main_rotate_bad"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="@dimen/d10"
                            android:text="@string/main_rotate_bad"
                            android:textColor="@color/black_54"
                            android:textSize="@dimen/s13" />

                        <TextView
                            android:id="@+id/main_rotate_good"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="@dimen/d10"
                            android:text="@string/main_rotate_good"
                            android:textColor="@color/A1"
                            android:textSize="@dimen/s13" />
                    </LinearLayout>

                </LinearLayout>

                <FrameLayout
                    android:id="@+id/ad_fl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:visibility="gone">
                    <!--ad-->
                    <LinearLayout
                        android:id="@+id/ll_ad_xiao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" />
                </FrameLayout>
                <!--功能块-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <!--垃圾清理-->
                    <LinearLayout
                        android:id="@+id/main_junk_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginLeft="@dimen/d10"
                        android:layout_marginRight="@dimen/d10"
                        android:background="@drawable/shape_ad"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d100"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="@dimen/d56"
                                android:layout_height="match_parent"
                                android:background="@drawable/shape_a3_l">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center"
                                    android:src="@mipmap/junk_card" />
                            </FrameLayout>


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d10"
                                android:layout_marginRight="@dimen/d10"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_junk"
                                    android:textColor="@color/black_87"
                                    android:textSize="@dimen/s16" />

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginTop="@dimen/d8"
                                    android:layout_weight="1"
                                    android:text="@string/success_8"
                                    android:textColor="@color/black_54"
                                    android:textSize="@dimen/s12" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginBottom="@dimen/d10"
                                    android:text="@string/junk_button"
                                    android:textColor="@color/A1"
                                    android:textSize="@dimen/s13" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <!--内存加速-->
                    <LinearLayout
                        android:id="@+id/main_ram_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginLeft="@dimen/d10"
                        android:layout_marginRight="@dimen/d10"
                        android:background="@drawable/shape_ad"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d100"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="@dimen/d56"
                                android:layout_height="match_parent"
                                android:background="@drawable/shape_a3_l">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center"
                                    android:src="@mipmap/ram_card" />
                            </FrameLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d10"
                                android:layout_marginRight="@dimen/d10"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_ram"
                                    android:textColor="@color/black_87"
                                    android:textSize="@dimen/s16"
                                     />

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginTop="@dimen/d8"
                                    android:layout_weight="1"
                                    android:text="@string/success_9"
                                    android:textColor="@color/black_54"
                                    android:textSize="@dimen/s12"
                                     />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginBottom="@dimen/d10"
                                    android:text="@string/ram_button"
                                    android:textColor="@color/A1"
                                    android:textSize="@dimen/s13" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <!--手机降温-->
                    <LinearLayout
                        android:id="@+id/main_cooling_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginLeft="@dimen/d10"
                        android:layout_marginRight="@dimen/d10"
                        android:background="@drawable/shape_ad"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d100"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="@dimen/d56"
                                android:layout_height="match_parent"
                                android:background="@drawable/shape_a3_l">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center"
                                    android:src="@mipmap/cpu_card" />
                            </FrameLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d10"
                                android:layout_marginRight="@dimen/d10"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/main_cooling_name"
                                    android:textColor="@color/black_87"
                                    android:textSize="@dimen/s16" />

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginTop="@dimen/d8"
                                    android:layout_weight="1"
                                    android:text="@string/success_10"
                                    android:textColor="@color/black_54"
                                    android:textSize="@dimen/s12"
                                     />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginBottom="@dimen/d10"
                                    android:text="@string/main_tab_3"
                                    android:textColor="@color/A1"
                                    android:textSize="@dimen/s13" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <!--相似图片-->
                    <LinearLayout
                        android:id="@+id/main_picture_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginLeft="@dimen/d10"
                        android:layout_marginRight="@dimen/d10"
                        android:background="@drawable/shape_ad"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d100"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="@dimen/d56"
                                android:layout_height="match_parent"
                                android:background="@drawable/shape_a3_l">

                                <ImageView
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center"
                                    android:src="@mipmap/picture_main" />
                            </FrameLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d10"
                                android:layout_marginRight="@dimen/d10"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:id="@+id/main_msg_picture"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d16"
                                    android:text="@string/side_picture"
                                    android:textColor="@color/black_87"
                                    android:textSize="@dimen/s16" />

                                <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_marginTop="@dimen/d8"
                                    android:layout_weight="1"
                                    android:text="@string/picture_delete"
                                    android:textColor="@color/black_54"
                                    android:textSize="@dimen/s12" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginBottom="@dimen/d10"
                                    android:text="@string/picture_button"
                                    android:textColor="@color/A1"
                                    android:textSize="@dimen/s13" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <!--深度清理-->
                    <LinearLayout
                        android:id="@+id/main_power_button"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d100"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginLeft="@dimen/d10"
                        android:layout_marginRight="@dimen/d10"
                        android:background="@drawable/shape_ad"
                        android:orientation="horizontal">

                        <FrameLayout
                            android:layout_width="@dimen/d56"
                            android:layout_height="match_parent"
                            android:background="@drawable/shape_a3_l">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center"
                                android:src="@mipmap/power_6" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/d10"
                            android:layout_marginRight="@dimen/d10"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d16"
                                android:text="@string/side_power"
                                android:textColor="@color/black_87"
                                android:textSize="@dimen/s12"
                                 />

                            <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/power_text"
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_marginTop="@dimen/d8"
                                android:layout_weight="1"
                                android:text="@string/power_3"
                                android:textColor="@color/black_54"
                                android:textSize="@dimen/s12"
                                 />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:layout_marginBottom="@dimen/d10"
                                android:text="@string/notification_button"
                                android:textColor="@color/A1"
                                android:textSize="@dimen/s13" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ad_native_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:visibility="gone" />


            </LinearLayout>
        </com.upupup.clean.junk.myview.SlowScrollView>
    </FrameLayout>
</LinearLayout>