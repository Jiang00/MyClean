<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/success"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A10"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/A1"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d72"
                android:src="@mipmap/success_duigou" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d33"
                android:text="@string/clean_success"
                android:textColor="@color/A8"
                android:textSize="@dimen/s17" />

        </LinearLayout>


        <com.mutter.clean.junk.myview.SlowScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:scrollbars="none"
            android:visibility="visible">
            <!--android:visibility="invisible"-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/A10"
                android:orientation="vertical">
                <!--tou-->

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d116"
                    android:layout_marginBottom="@dimen/d10"
                    android:background="@color/A1">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/d50"
                            android:layout_height="@dimen/d56"
                            android:layout_marginLeft="@dimen/d72"
                            android:src="@mipmap/success_duigou" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/d20"
                            android:orientation="vertical">

                            <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/success_clean_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/success_cleaned"
                                android:textColor="@color/A8"
                                android:textSize="@dimen/s17"
                                app:typeface="lyp_text.otf" />

                            <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/success_clean_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d8"
                                android:text="@string/success_1"
                                android:textColor="@color/A8"
                                android:textSize="@dimen/s17"
                                app:typeface="lyp_text.otf" />
                        </LinearLayout>
                    </LinearLayout>


                </FrameLayout>

                <!--好评-->
                <LinearLayout
                    android:id="@+id/main_rotate_all"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:elevation="@dimen/d2"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d106"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="@dimen/d16"
                                android:text="@string/rotate_1"
                                android:textColor="@color/A6"
                                android:textSize="@dimen/s14" />

                            <ImageView
                                android:id="@+id/main_rotate_cha"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:layout_marginRight="@dimen/d3"
                                android:layout_marginTop="@dimen/d3"
                                android:src="@mipmap/rotate_cha" />
                        </FrameLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="@dimen/d36"
                                android:layout_marginRight="@dimen/d17"
                                android:src="@mipmap/main_rotate_2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:text="@string/rotate_2"
                                android:textColor="@color/B3"
                                android:textSize="@dimen/s16" />
                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d12"
                        android:layout_marginTop="@dimen/d16"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/main_rotate_bad"
                            android:layout_width="@dimen/d112"
                            android:layout_height="@dimen/d29"
                            android:background="@drawable/shape_rotate_bad"
                            android:gravity="center"
                            android:text="@string/main_rotate_bad"
                            android:textColor="@color/B3"
                            android:textSize="@dimen/s13" />

                        <TextView
                            android:id="@+id/main_rotate_good"
                            android:layout_width="@dimen/d112"
                            android:layout_height="@dimen/d29"
                            android:background="@drawable/shape_rotate_good"
                            android:gravity="center"
                            android:text="@string/main_rotate_good"
                            android:textColor="@color/A8"
                            android:textSize="@dimen/s13" />
                    </LinearLayout>

                </LinearLayout>

                <!--ad-->
                <LinearLayout
                    android:id="@+id/ll_ad_xiao"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:elevation="@dimen/d2"
                    android:orientation="vertical"
                    android:visibility="gone" />

                <!--功能块-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:elevation="@dimen/d2"
                    android:orientation="vertical">
                    <!--文件管理-->
                    <LinearLayout
                        android:id="@+id/main_file_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/file_main" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:id="@+id/main_msg_file"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_file"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/file_main"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--垃圾清理-->
                    <LinearLayout
                        android:id="@+id/main_junk_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/junk_card" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_junk"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/success_8"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--内存加速-->
                    <LinearLayout
                        android:id="@+id/main_ram_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/ram_card" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_ram"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/success_9"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--手机降温-->
                    <LinearLayout
                        android:id="@+id/main_cooling_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/cpu_card" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/main_cooling_name"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/success_10"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--游戏加速-->
                    <LinearLayout
                        android:id="@+id/main_gboost_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/gboost_main" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:id="@+id/main_msg_GBoost"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/gboost_0"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/gboost_10"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--相似图片-->
                    <LinearLayout
                        android:id="@+id/main_picture_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/picture_main" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:id="@+id/main_msg_picture"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_picture"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/picture_delete"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--清理通知栏-->
                    <LinearLayout
                        android:id="@+id/main_notifi_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:background="?attr/selectableItemBackground"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d16"
                                android:src="@mipmap/notifi_6" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:id="@+id/main_msg_notifi"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_notifi"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s14"
                                    app:typeface="lyp_text.otf" />

                                <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d8"
                                    android:text="@string/notifi_msg_main"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s12"
                                    app:typeface="lyp_text.otf" />
                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>


                    <!--深度清理-->
                    <LinearLayout
                        android:id="@+id/main_power_button"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d72"
                        android:background="?attr/selectableItemBackground"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/power_icon"
                            android:layout_width="@dimen/d40"
                            android:layout_height="@dimen/d40"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/d16"
                            android:src="@mipmap/icon" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/d16"
                            android:layout_marginRight="@dimen/d8"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/side_power"
                                android:textColor="@color/B2"
                                android:textSize="@dimen/s14"
                                app:typeface="lyp_text.otf" />

                            <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                                android:id="@+id/power_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d8"
                                android:text="@string/power_3"
                                android:textColor="@color/B3"
                                android:textSize="@dimen/s12"
                                app:typeface="lyp_text.otf" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ad_native_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d10"
                    android:layout_marginLeft="@dimen/d10"
                    android:layout_marginRight="@dimen/d10"
                    android:background="@drawable/shape_ad"
                    android:elevation="@dimen/d2"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:visibility="gone" />


            </LinearLayout>
        </com.mutter.clean.junk.myview.SlowScrollView>
    </FrameLayout>
</LinearLayout>