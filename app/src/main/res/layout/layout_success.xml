<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent">

    <LinearLayout
        android:id="@+id/success"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/app_color_first"
        android:orientation="vertical">
        <!--android:visibility="invisible"-->
        <LinearLayout
            android:id="@+id/rl_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/app_color_first"
            android:orientation="vertical">

            <View
                android:id="@+id/view_title_bar"
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <include layout="@layout/layout_title" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="208dp"
                android:layout_height="208dp"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="250dp"
                android:src="@mipmap/success_1" />

            <ImageView
                android:layout_width="208dp"
                android:layout_height="208dp"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="250dp"
                android:src="@mipmap/success_2" />

            <ImageView
                android:id="@+id/success_progress"
                android:layout_width="208dp"
                android:layout_height="208dp"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="250dp"
                android:src="@mipmap/success_3" />

            <com.supers.clean.junk.customeview.DrawHookView
                android:id="@+id/success_drawhook"
                android:layout_width="208dp"
                android:layout_height="208dp"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="250dp" />

            <ImageView
                android:id="@+id/success_huojian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="250dp"
                android:src="@mipmap/success_huojian"
                android:visibility="invisible" />

            <com.supers.clean.junk.customeview.SlowScrollView
                android:id="@+id/scrollView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                android:scrollbars="none"
                android:visibility="invisible">
                <!--android:visibility="invisible"-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#e8e8e8"
                    android:orientation="vertical">
                    <!--tou-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="117dp"
                        android:background="@mipmap/success_title_backg"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="17dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="40dp"
                                android:src="@mipmap/success_duigou" />

                            <TextView
                                android:id="@+id/success_clean_size"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="13dp"
                                android:text="@string/success_cleaned"
                                android:textColor="@color/white_100"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/success_jiantou"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="11dp"
                            android:src="@mipmap/success_tt" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_rotate_all"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d16"
                        android:layout_marginTop="@dimen/d16"
                        android:background="@color/white_100"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:scaleType="centerCrop"
                                android:src="@mipmap/main_rotate_2" />

                            <ImageView
                                android:id="@+id/delete"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:src="@mipmap/delete" />
                        </FrameLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="53dp"
                            android:layout_marginRight="53dp"
                            android:layout_marginTop="18dp"
                            android:gravity="center"
                            android:text="@string/main_rotate_msg"
                            android:textColor="#1e88e5"
                            android:textSize="17sp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="30dp"
                            android:background="#e5e5e5" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/main_rotate_bad"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:background="?attr/selectableItemBackground"
                                android:gravity="center"
                                android:text="@string/main_rotate_bad"
                                android:textColor="@color/black_54"
                                android:textSize="17sp" />

                            <LinearLayout
                                android:id="@+id/main_rotate_good"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:background="?attr/selectableItemBackground"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/main_rotate_good"
                                    android:textColor="@color/black_54"
                                    android:textSize="17sp" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="3dp"
                                    android:src="@mipmap/main_rotate_3" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_ad_xiao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="16dp"
                        android:background="@color/white_100"
                        android:orientation="vertical"
                        android:visibility="gone" />

                    <!--文件管理-->
                    <LinearLayout
                        android:id="@+id/main_file_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/d40"
                            android:layout_height="@dimen/d40"
                            android:layout_marginLeft="@dimen/d20"
                            android:layout_marginTop="@dimen/d16"
                            android:src="@mipmap/file_main" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d15"
                            android:layout_marginLeft="@dimen/d12"
                            android:layout_marginTop="@dimen/d16"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/side_file"
                                android:textColor="#45527a"
                                android:textSize="@dimen/s16" />

                            <TextView
                                android:layout_width="@dimen/d225"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d4"
                                android:text="@string/file_main"
                                android:textColor="#828791"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d29"
                            android:src="@mipmap/right" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f1f1f1" />
                    <!--相似图片-->
                    <LinearLayout
                        android:id="@+id/main_picture_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/d40"
                            android:layout_height="@dimen/d40"
                            android:layout_marginLeft="@dimen/d20"
                            android:layout_marginTop="@dimen/d16"
                            android:src="@mipmap/file_main" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d15"
                            android:layout_marginLeft="@dimen/d12"
                            android:layout_marginTop="@dimen/d16"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/side_file"
                                android:textColor="#45527a"
                                android:textSize="@dimen/s16" />

                            <TextView
                                android:layout_width="@dimen/d225"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d4"
                                android:text="@string/file_main"
                                android:textColor="#828791"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d29"
                            android:src="@mipmap/right" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f1f1f1" />
                    <!--清理通知栏-->
                    <LinearLayout
                        android:id="@+id/main_notifi_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/notifi_icon"
                            android:layout_width="@dimen/d40"
                            android:layout_height="@dimen/d40"
                            android:layout_marginLeft="@dimen/d20"
                            android:layout_marginTop="@dimen/d16"
                            android:src="@mipmap/notifi_6" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d15"
                            android:layout_marginLeft="@dimen/d12"
                            android:layout_marginTop="@dimen/d16"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/side_notifi"
                                android:textColor="#45527a"
                                android:textSize="@dimen/s16" />

                            <TextView
                                android:id="@+id/notifi_text"
                                android:layout_width="@dimen/d225"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d4"
                                android:text="@string/notifi_msg_main"
                                android:textColor="#828791"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d29"
                            android:src="@mipmap/right" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f1f1f1" />

                    <!--深度清理-->
                    <LinearLayout
                        android:id="@+id/main_power_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/power_icon"
                            android:layout_width="@dimen/d40"
                            android:layout_height="@dimen/d40"
                            android:layout_marginLeft="@dimen/d20"
                            android:layout_marginTop="@dimen/d16"
                            android:src="@mipmap/icon" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d15"
                            android:layout_marginLeft="@dimen/d12"
                            android:layout_marginTop="@dimen/d16"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/side_power"
                                android:textColor="#45527a"
                                android:textSize="@dimen/s16" />

                            <TextView
                                android:id="@+id/power_text"
                                android:layout_width="@dimen/d225"
                                android:layout_height="wrap_content"
                                android:text="@string/power_3"
                                android:textColor="#828791"
                                android:textSize="@dimen/s14" />
                        </LinearLayout>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d29"
                            android:src="@mipmap/right" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#f1f1f1" />

                    <!--交叉推广-->
                    <LinearLayout
                        android:id="@+id/main_tuiguang_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="#ffffff"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d15"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d19"
                                android:src="@mipmap/main_rotate_1" />

                            <TextView
                                android:id="@+id/main_msg_tuiguang"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="@dimen/d5"
                                android:textColor="@color/black_87"
                                android:textSize="@dimen/s16" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="@dimen/d16"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/main_4"
                            android:background="#f1f1f1" />

                        <FrameLayout
                            android:id="@+id/fl_lot_success"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d135">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom|right"
                                android:background="@color/black_35"
                                android:fontFamily="sans-serif-light"
                                android:gravity="center"
                                android:text="AD"
                                android:textColor="@android:color/white"
                                android:textSize="8sp" />
                        </FrameLayout>


                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d16"
                            android:background="#e8e8e8" />
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/ad_native_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="@color/app_color_first"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:visibility="invisible">

                    </LinearLayout>

                </LinearLayout>
            </com.supers.clean.junk.customeview.SlowScrollView>
        </FrameLayout>
    </LinearLayout>
</FrameLayout>