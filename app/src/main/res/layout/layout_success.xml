                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_junk"
                                    android:textColor="#2f2f2f"
                                    android:textSize="@dimen/s16"
                                    app:typeface="ljh_1.TTF" />

                                <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d3"
                                    android:text="@string/success_8"
                                    android:textColor="#802f2f2f"
                                    android:textSize="@dimen/s12"
                                    app:typeface="ljh_1.TTF" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--内存加速-->
