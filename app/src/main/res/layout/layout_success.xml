                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d13"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d46"
                                android:layout_height="@dimen/d46"
                                android:layout_marginLeft="@dimen/d82"
                                android:src="@mipmap/success_duigou" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/d24"
                                android:orientation="vertical">

                                <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                    android:id="@+id/success_clean_size"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/success_cleaned"
                                    android:textColor="@color/A8"
                                    android:textSize="@dimen/s15"
                                    app:typeface="ljh_1.TTF" />

                                <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                    android:id="@+id/success_clean_2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d5"
                                    android:text="@string/success_1"
                                    android:textColor="@color/A8"
                                    android:textSize="@dimen/s15"
                                    app:typeface="ljh_1.TTF" />
                            </LinearLayout>


                        </LinearLayout>

                        <ImageView
                            android:id="@+id/success_jiantou"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginBottom="@dimen/d5"
                            android:layout_marginTop="@dimen/d20"
                            android:src="@mipmap/success_tt" />

                    </LinearLayout>

                    <!--好评-->
                    <LinearLayout
