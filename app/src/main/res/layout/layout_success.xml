                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d16"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_ram"
                                    android:textColor="#2f2f2f"
                                    android:textSize="@dimen/s16"
                                    app:typeface="ljh_1.TTF" />

                                <com.froumobic.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d3"
                                    android:text="@string/success_9"
                                    android:textColor="#802f2f2f"
                                    android:textSize="@dimen/s12"
                                    app:typeface="ljh_1.TTF" />
                            </LinearLayout>

                            <ImageView
