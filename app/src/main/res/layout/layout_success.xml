<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent">

    <LinearLayout
        android:id="@+id/success"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/A1"
        android:orientation="vertical">
        <!--android:visibility="invisible"-->
        <LinearLayout
            android:id="@+id/rl_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/A1"
            android:orientation="vertical">

            <View
                android:id="@+id/view_title_bar"
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <include layout="@layout/layout_title" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_1" />

            <ImageView
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_2" />

            <ImageView
                android:id="@+id/success_progress"
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_3" />

            <com.supers.clean.junk.customeview.DrawHookView
                android:id="@+id/success_drawhook"
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250" />

            <ImageView
                android:id="@+id/success_huojian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_huojian"
                android:visibility="invisible" />

            <com.supers.clean.junk.customeview.SlowScrollView
                android:id="@+id/scrollView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                android:scrollbars="none"
                android:visibility="visible">
                <!--android:visibility="invisible"-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/A10"
                    android:orientation="vertical">
                    <!--tou-->
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d121"
                        android:background="@color/A1">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/d121"
                            android:layout_gravity="right|center_vertical"
                            android:src="@mipmap/success_title_backg" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/d17"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="40dp"
                                    android:src="@mipmap/success_duigou" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="13dp"
                                    android:orientation="vertical">

                                    <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                        android:id="@+id/success_clean_size"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/success_cleaned"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16"
                                        app:typeface="eos_clean.otf" />

                                    <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                        android:id="@+id/success_clean_2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/success_1"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16"
                                        app:typeface="eos_clean.otf" />
                                </LinearLayout>


                            </LinearLayout>

                            <ImageView
                                android:id="@+id/success_jiantou"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="@dimen/d15"
                                android:src="@mipmap/success_tt" />

                        </LinearLayout>
                    </FrameLayout>

                    <!--好评-->
                    <LinearLayout
                        android:id="@+id/main_rotate_all"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d9"
                        android:background="@color/white_100"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d121"
                                android:layout_marginLeft="@dimen/d16"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/main_rotate_2" />

                            <ImageView
                                android:id="@+id/rotate_ic"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/d61"
                                android:layout_gravity="center_horizontal|bottom"
                                android:src="@mipmap/main_rotate_3" />

                            <ImageView
                                android:id="@+id/rotate_delete"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:src="@mipmap/rotate_cha" />
                        </FrameLayout>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/d16"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d23"
                            android:gravity="center"
                            android:text="@string/rotate_1"
                            android:textColor="@color/black_87"
                            android:textSize="@dimen/s17" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/d16"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d14"
                            android:gravity="center"
                            android:text="@string/rotate_2"
                            android:textColor="@color/black_54"
                            android:textSize="@dimen/s14" />

                        <Button
                            android:id="@+id/main_rotate_good"
                            android:layout_width="@dimen/d250"
                            android:layout_height="@dimen/d37"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginBottom="@dimen/d16"
                            android:layout_marginTop="@dimen/d24"
                            android:background="@drawable/shape_a1_r"
                            android:text="@string/rotate_3"
                            android:textColor="@color/white_100"
                            android:textSize="@dimen/s17" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_ad_xiao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/d9"
                        android:background="@color/A8"
                        android:orientation="vertical"
                        android:visibility="gone" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d9" />
                    <!--文件管理-->
                    <LinearLayout
                        android:id="@+id/main_file_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/A8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/file_main" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_file"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/file_main"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--垃圾清理-->
                    <LinearLayout
                        android:id="@+id/main_junk_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/A8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/junk_card" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_junk"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/success_8"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--内存加速-->
                    <LinearLayout
                        android:id="@+id/main_ram_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/A8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/ram_card" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_ram"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/success_9"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--手机降温-->
                    <LinearLayout
                        android:id="@+id/main_cooling_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/A8"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView3"
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/cpu_card" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/main_cooling_name"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/success_10"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>


                    <!--游戏加速-->
                    <LinearLayout
                        android:id="@+id/main_gboost_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#ffffff"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/gboost_main" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/gboost_0"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/gboost_10"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>

                    <!--相似图片-->
                    <LinearLayout
                        android:id="@+id/main_picture_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#ffffff"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/main_picture" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_picture"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/picture_delete"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>


                    <!--清理通知栏-->
                    <LinearLayout
                        android:id="@+id/main_notifi_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#ffffff"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/notifi_icon"
                                android:layout_width="@dimen/d40"
                                android:layout_height="@dimen/d40"
                                android:layout_marginLeft="@dimen/d20"
                                android:layout_marginTop="@dimen/d16"
                                android:src="@mipmap/notifi_6" />

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/d15"
                                android:layout_marginLeft="@dimen/d12"
                                android:layout_marginTop="@dimen/d16"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_notifi"
                                    android:textColor="@color/B2"
                                    android:textSize="@dimen/s16"
                                    app:typeface="eos_clean.otf" />

                                <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                    android:id="@+id/notifi_text"
                                    android:layout_width="@dimen/d225"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/d4"
                                    android:text="@string/notifi_msg_main"
                                    android:textColor="@color/B3"
                                    android:textSize="@dimen/s14"
                                    app:typeface="eos_clean.otf" />
                            </LinearLayout>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/d16"
                                android:layout_marginTop="@dimen/d29"
                                android:src="@mipmap/right" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/B1" />
                    </LinearLayout>


                    <!--深度清理-->
                    <LinearLayout
                        android:id="@+id/main_power_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/power_icon"
                            android:layout_width="@dimen/d40"
                            android:layout_height="@dimen/d40"
                            android:layout_marginLeft="@dimen/d20"
                            android:layout_marginTop="@dimen/d16"
                            android:src="@mipmap/icon" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/d15"
                            android:layout_marginLeft="@dimen/d12"
                            android:layout_marginTop="@dimen/d16"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/side_power"
                                android:textColor="@color/B2"
                                android:textSize="@dimen/s16"
                                app:typeface="eos_clean.otf" />

                            <com.eos.module.charge.saver.view.CustomerTypefaceTextView
                                android:id="@+id/power_text"
                                android:layout_width="@dimen/d225"
                                android:layout_height="wrap_content"
                                android:text="@string/power_3"
                                android:textColor="@color/B3"
                                android:textSize="@dimen/s14"
                                app:typeface="eos_clean.otf" />
                        </LinearLayout>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/d16"
                            android:layout_marginTop="@dimen/d29"
                            android:src="@mipmap/right" />
                    </LinearLayout>


                    <!--交叉推广-->
                    <LinearLayout
