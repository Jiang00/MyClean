<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent">

    <LinearLayout
        android:id="@+id/success"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/A1"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/rl_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:id="@+id/view_title_bar"
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <include layout="@layout/layout_title" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_1" />


            <ImageView
                android:id="@+id/success_progress"
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_3" />

            <com.fast.clean.junk.myview.DrawHookView
                android:id="@+id/success_drawhook"
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250" />

            <ImageView
                android:id="@+id/success_huojian"
                android:layout_width="@dimen/d208"
                android:layout_height="@dimen/d208"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="@dimen/d250"
                android:src="@mipmap/success_huojian"
                android:visibility="invisible" />

            <com.fast.clean.junk.myview.SlowScrollView
                android:id="@+id/scrollView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/A7"
                android:fillViewport="true"
                android:scrollbars="none"
                android:visibility="invisible">
                <!--android:visibility="invisible"-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/A13"
                    android:orientation="vertical">
                    <!--tou-->
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/d129"
                        android:layout_marginBottom="@dimen/d9"
                        android:background="@color/A1">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right|bottom"
                            android:src="@mipmap/success_title_backg" />

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d29"
                                    android:src="@mipmap/success_duigou" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/d10"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:id="@+id/success_clean_size"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/success_cleaned"
                                        android:textColor="@color/A7"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:id="@+id/success_clean_2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/success_1"
                                        android:textColor="@color/A7"
                                        android:textSize="@dimen/s16" />
                                </LinearLayout>


                            </LinearLayout>

                            <ImageView
                                android:id="@+id/success_jiantou"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom|center_horizontal"
                                android:layout_marginBottom="@dimen/d6"
                                android:src="@mipmap/success_tt" />

                        </FrameLayout>
                    </FrameLayout>

                    <!--好评-->
                    <LinearLayout
                        android:id="@+id/main_rotate_all"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d9"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:background="@drawable/shape_ffffff_r"
                        android:orientation="vertical">


                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d84"
                            android:scaleType="centerCrop"
                            android:src="@mipmap/main_rotate_2" />

                        <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginBottom="@dimen/d27"
                            android:layout_marginTop="@dimen/d20"
                            android:text="@string/rotate_msg"
                            android:textColor="@color/A8"
                            android:textSize="@dimen/s16" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d43"
                            android:orientation="horizontal">

                            <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                android:id="@+id/main_rotate_bad"
                                android:layout_width="@dimen/d121"
                                android:layout_height="@dimen/d36"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginLeft="@dimen/d24"
                                android:gravity="center"
                                android:text="@string/rotate_cancel"
                                android:textColor="@color/A9"
                                android:textSize="@dimen/s16" />

                            <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                android:id="@+id/main_rotate_good"
                                android:layout_width="@dimen/d176"
                                android:layout_height="@dimen/d36"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginLeft="@dimen/d8"
                                android:background="@drawable/shape__a4_r"
                                android:elevation="@dimen/d2"
                                android:gravity="center"
                                android:text="@string/rotate_ok"
                                android:textColor="@color/A7"
                                android:textSize="@dimen/s16"
                                app:typeface="Roboto-Medium.ttf" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_ad_xiao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d9"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:background="@drawable/shape_ad"
                        android:orientation="vertical"
                        android:visibility="gone" />


                    <!--功能块-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d9"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:background="@drawable/shape_ffffff_r"
                        android:orientation="vertical">
                        <!--文件管理-->
                        <LinearLayout
                            android:id="@+id/main_file_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/file_main" />
                                </FrameLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_file"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/file_main"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>

                        <!--垃圾清理-->
                        <LinearLayout
                            android:id="@+id/main_junk_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/junk_card" />
                                </FrameLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_junk"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/success_8"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>

                        <!--内存加速-->
                        <LinearLayout
                            android:id="@+id/main_ram_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/ram_card" />
                                </FrameLayout>


                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_ram"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/success_9"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>

                        <!--手机降温-->
                        <LinearLayout
                            android:id="@+id/main_cooling_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/cpu_card" />
                                </FrameLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/main_cooling_name"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/success_10"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>


                        <!--游戏加速-->
                        <LinearLayout
                            android:id="@+id/main_gboost_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/gboost_main" />
                                </FrameLayout>


                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/gboost_0"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/gboost_10"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>

                        <!--相似图片-->
                        <LinearLayout
                            android:id="@+id/main_picture_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/picture_main" />
                                </FrameLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_picture"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/picture_delete"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>


                        <!--清理通知栏-->
                        <LinearLayout
                            android:id="@+id/main_notifi_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/d72"
                                android:background="#ffffff"
                                android:orientation="horizontal">

                                <FrameLayout
                                    android:layout_width="@dimen/d72"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/notifi_icon"
                                        android:layout_width="@dimen/d40"
                                        android:layout_height="@dimen/d40"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="@dimen/d16"
                                        android:src="@mipmap/notifi_6" />
                                </FrameLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical">

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/side_notifi"
                                        android:textColor="@color/A8"
                                        android:textSize="@dimen/s16" />

                                    <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                        android:id="@+id/notifi_text"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/d4"
                                        android:text="@string/notifi_msg_main"
                                        android:textColor="@color/A9"
                                        android:textSize="@dimen/s14" />
                                </LinearLayout>

                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:background="@color/A13" />
                        </LinearLayout>


                        <!--深度清理-->
                        <LinearLayout
                            android:id="@+id/main_power_button"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/d72"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="@dimen/d72"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:id="@+id/power_icon"
                                    android:layout_width="@dimen/d40"
                                    android:layout_height="@dimen/d40"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/d16"
                                    android:src="@mipmap/power_6" />
                            </FrameLayout>


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/side_power"
                                    android:textColor="@color/A8"
                                    android:textSize="@dimen/s16" />

                                <com.charging.module.charge.saver.view.CustomerTypefaceTextView
                                    android:id="@+id/power_text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/power_3"
                                    android:textColor="@color/A9"
                                    android:textSize="@dimen/s14" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <!--大屏native-->
                    <LinearLayout
                        android:id="@+id/ad_native_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/d9"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_marginRight="@dimen/d8"
                        android:background="@drawable/shape_ad"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:visibility="gone" />


                </LinearLayout>
            </com.fast.clean.junk.myview.SlowScrollView>
        </FrameLayout>
    </LinearLayout>
</FrameLayout>