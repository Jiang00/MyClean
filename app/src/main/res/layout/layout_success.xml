            android:layout_height="match_parent">

            <com.froumobic.clean.junk.mview.DrawHookView
                android:id="@+id/success_drawhook"
                android:layout_width="@dimen/d109"
                android:layout_height="@dimen/d109"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/d72" />

            <TextView
