<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/gboost_item_c"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/selectableItemBackground">

    <LinearLayout
        android:id="@+id/linearlayou_add"
        android:layout_width="@dimen/d90"
        android:layout_height="@dimen/d108"
        android:layout_gravity="center"
        android:background="@drawable/shape_button_b3"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/gboost_item_add"
            android:layout_width="@dimen/d46"
            android:layout_height="@dimen/d46" />

        <com.icleaner.module.charge.saver.protectview.CustomerTypefaceTextView
            android:id="@+id/gboost_item_addname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d8"
            android:gravity="center"
            android:text="aaaa"
            android:textColor="@color/A8"
            android:textSize="@dimen/s11" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearlayout_qita"
        android:layout_width="@dimen/d95"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/shape_button_b3"
        android:orientation="vertical"
        android:visibility="gone">

        <ImageView
            android:id="@+id/gboost_item_icon"
            android:layout_width="@dimen/d46"
            android:layout_height="@dimen/d46"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/d8" />

        <com.icleaner.module.charge.saver.protectview.CustomerTypefaceTextView
            android:id="@+id/gboost_item_name"
            android:layout_width="@dimen/d46"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/d3"
            android:layout_marginTop="@dimen/d6"
            android:gravity="center"
            android:singleLine="true"
            android:text="aaaa"
            android:textColor="@color/A1"
            android:textSize="@dimen/s12" />

        <TextView
            android:id="@+id/gboost_item_textview"
            android:layout_width="@dimen/d54"
            android:layout_height="@dimen/d21"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/d9"
            android:background="@drawable/shape_button_a8"
            android:gravity="center"
            android:text="@string/booster"
            android:textColor="#ffffff"
            android:textSize="@dimen/s10" />
    </LinearLayout>
</FrameLayout>