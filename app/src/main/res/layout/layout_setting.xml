<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/backage"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/junk_title_backg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>


    <ScrollView
        android:id="@+id/setting_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/ad_fl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d7"
                android:layout_marginRight="@dimen/d7"
                android:layout_marginTop="@dimen/d7"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/ll_ad"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" />
            </FrameLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d16"
                        android:gravity="center_vertical"
                        android:text="@string/setting_kaiguanllei"
                        android:textColor="@color/A3"
                        android:textSize="@dimen/s12" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/setting_battery"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/battery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_battery"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_battery_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/setting_tongzhi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_tongzhi"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_tongzhi_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_tongzhilan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_tongzhilan"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_tongzhilan_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/setting_auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_auto"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_auto_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_float"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_float"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_float_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_unload"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible">

                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_unload"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:id="@+id/setting_unload_check"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true"
                        android:scaleType="center" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/d33"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d16"
                        android:gravity="center_vertical"
                        android:text="@string/setting_gongneng"
                        android:textColor="@color/A3"
                        android:textSize="@dimen/s12" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/setting_picture"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56"
                    android:visibility="gone">

                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/side_picture"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_hui"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56"
                    android:visibility="gone">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/picture_hui"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_gboost"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56"
                    android:visibility="gone">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/gboost_0"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_file"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56"
                    android:visibility="gone">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/side_file"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_notifi"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56"
                    android:visibility="gone">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/side_notifi"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_power"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56"
                    android:visibility="gone">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/side_power"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_white"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_white"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_short"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_short"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_rotate"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56">


                    <com.upupup.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_rotate"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/s14" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />
                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>
