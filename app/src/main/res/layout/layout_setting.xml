                <RelativeLayout
                    android:id="@+id/setting_white"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_white"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <ImageView
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_short"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
