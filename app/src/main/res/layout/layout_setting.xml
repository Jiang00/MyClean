                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/gboost_0"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <ImageView
                        android:id="@+id/c"
                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                    <TextView
                        android:layout_width="@dimen/d30"
                        android:layout_height="@dimen/d14"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/c"
