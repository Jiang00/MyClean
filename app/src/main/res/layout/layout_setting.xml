                        android:layout_width="@dimen/d36"
                        android:layout_height="@dimen/d36"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/d16"
                        android:src="@mipmap/setting_rig" />

                    <TextView
                        android:layout_width="@dimen/d30"
                        android:layout_height="@dimen/d14"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/a"
                        android:background="@drawable/shape_a2"
                        android:gravity="center"
                        android:text="NEW"
                        android:textColor="@color/A8"
                        android:textSize="@dimen/s8" />
                </RelativeLayout>


                <RelativeLayout
