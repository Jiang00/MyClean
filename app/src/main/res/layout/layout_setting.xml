                <RelativeLayout
                    android:id="@+id/setting_notifi"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d56">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
