<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/A10"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/junk_title_backg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/A1"
        android:orientation="vertical">

        <View
            android:id="@+id/view_title_bar"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <include layout="@layout/layout_title" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_ad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/d7"
        android:background="@drawable/shape_ad"
        android:elevation="@dimen/d2"
        android:orientation="vertical"
        android:visibility="gone" />

    <ScrollView
        android:id="@+id/setting_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d7"
                android:background="@color/A8"
                android:elevation="@dimen/d2"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/d32"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/d2"
                        android:layout_height="@dimen/d13"
                        android:layout_marginLeft="@dimen/d16"
                        android:src="@mipmap/setting_line" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d5"
                        android:gravity="center_vertical"
                        android:text="@string/setting_kaiguanllei"
                        android:textColor="@color/A5"
                        android:textSize="@dimen/s13"
                        app:typeface="lyp_text.otf" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/setting_tongzhi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_tongzhi"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_tongzhi_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_tongzhilan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_tongzhilan"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_tongzhilan_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_float"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_float"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_float_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_battery"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:id="@+id/battery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_battery"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d8"
                        android:layout_toRightOf="@id/battery"
                        android:text="@string/battery_9"
                        android:textColor="@color/B3"
                        android:textSize="@dimen/s9"
                        app:typeface="lyp_text.otf" />

                    <FrameLayout
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/setting_battery_check"
                            android:layout_width="@dimen/d36"
                            android:layout_height="@dimen/d36"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/setting_unload"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">


                    <com.mutter.module.charge.saver.Aview.CustomerTypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/d16"
                        android:text="@string/setting_unload"
                        android:textColor="@color/B2"
                        android:textSize="@dimen/s14"
                        app:typeface="lyp_text.otf" />

                    <ImageView
                        android:id="@+id/setting_unload_check"
                        android:layout_width="@dimen/d72"
                        android:layout_height="@dimen/d56"

                        android:layout_alignParentRight="true"
                        android:scaleType="center" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
