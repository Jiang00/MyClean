<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d6000000"
    android:orientation="vertical">

    <View
        android:id="@+id/view_title_bar"
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/d16"
        android:layout_marginRight="@dimen/d16"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/d176">

            <View
                android:layout_width="@dimen/d109"
                android:layout_height="@dimen/d109"
                android:layout_gravity="bottom|center_horizontal"
                android:background="@drawable/shape_ffffff_round" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/d141"
                android:background="@drawable/shape_ffffff_r">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/d18">

                    <LinearLayout
                        android:id="@+id/ll_liuliang"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_liuliang"
                            android:layout_width="@dimen/d48"
                            android:layout_height="@dimen/d48"
                            android:src="@mipmap/float_liuliang" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/tv_liuliang"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d4"
                            android:lines="1"
                            android:text="@string/float_liuliang"
                            android:textColor="@color/Z1"
                            android:textSize="@dimen/s12" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_wifi"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_wifi"
                            android:layout_width="@dimen/d48"
                            android:layout_height="@dimen/d48"
                            android:src="@mipmap/float_wifi" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/tv_wifi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d4"
                            android:lines="1"
                            android:text="@string/float_wifi"
                            android:textColor="@color/A8"
                            android:textSize="@dimen/s10" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_gps"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_gps"
                            android:layout_width="@dimen/d48"
                            android:layout_height="@dimen/d48"
                            android:src="@mipmap/float_gps" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/tv_gps"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d4"
                            android:lines="1"
                            android:text="@string/float_gps"
                            android:textColor="@color/A8"
                            android:textSize="@dimen/s10" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_shengyin"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_shengyin"
                            android:layout_width="@dimen/d48"
                            android:layout_height="@dimen/d48"
                            android:src="@mipmap/float_shengyin" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/tv_shengyin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d4"
                            android:lines="1"
                            android:text="@string/float_shengyin"
                            android:textColor="@color/A8"
                            android:textSize="@dimen/s10" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_xianshi"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/iv_xianshi"
                            android:layout_width="@dimen/d48"
                            android:layout_height="@dimen/d48"
                            android:src="@mipmap/float_liangdu_0" />

                        <com.security.module.mView.CustomerTypefaceTextView
                            android:id="@+id/tv_xianshi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/d4"
                            android:lines="1"
                            android:text="@string/float_liangdu"
                            android:textColor="@color/A8"
                            android:textSize="@dimen/s10" />
                    </LinearLayout>

                </LinearLayout>

            </FrameLayout>

            <FrameLayout
                android:layout_width="@dimen/d109"
                android:layout_height="@dimen/d109"
                android:layout_gravity="bottom|center_horizontal">

                <ImageView
                    android:id="@+id/float_jiasu"
                    android:layout_width="@dimen/d72"
                    android:layout_height="@dimen/d72"
                    android:layout_gravity="center"
                    android:src="@mipmap/float_1" />


                <ImageView
                    android:layout_width="@dimen/d72"
                    android:layout_height="@dimen/d72"
                    android:layout_gravity="center"
                    android:src="@mipmap/float_2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/notifi_jiasu"
                    android:textColor="@color/A1"
                    android:textSize="@dimen/s14" />
            </FrameLayout>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/ll_ad"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d10"
            android:background="@drawable/shape_a1_r"
            android:orientation="vertical">

        </LinearLayout>
    </LinearLayout>

</FrameLayout>