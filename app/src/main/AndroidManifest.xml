<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.froumobic.clean.junk">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <!--IMEI-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 读写SDCard权限 -->
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--获取最上层包名-->
    <uses-permission android:name="android.permission.GET_TASKS" />
    <!--可以访问使用量数据-->
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!--快捷方式-->
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />


    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <!--开机自启-->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <uses-permission
        android:name="android.permission.WRITE_MEDIA_STORAGE"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />


    <application
        android:name="com.froumobic.clean.junk.mactivity.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:overrideLibrary="com.mopub.mobileads"
        tools:replace="android:theme,android:supportsRtl,android:icon">


        <activity
            android:name="com.froumobic.clean.junk.mactivity.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="com.fraumobi.clean.main" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.froumobic.clean.junk.mactivity.LoadingActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.MEDIA_UNMOUNTED" />

                <data android:scheme="file" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.froumobic.clean.junk.mactivity.CoolingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.LajiActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.fraumobi.clean.LajiActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.froumobic.clean.junk.mactivity.LajjiAndRamActivity"

            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.AppActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.MessageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.RamAvtivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.fraumobi.clean.RamAvtivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.froumobic.clean.junk.mactivity.SettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.SuccessActivity"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.froumobic.clean.junk.mactivity.WhiteListAvtivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.WhiteListAddActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.DeepActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.NotifiAnimaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.PermissionActivity"
            android:allowTaskReparenting="true"
            android:excludeFromRecents="true"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.NotifiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.NotifiSettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.XuanfuActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.UnloadActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />

        <activity
            android:name="com.froumobic.clean.junk.mactivity.PhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.PhotoHuiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.GBoostActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.froumobic.clean.junk.mactivity.ShortCutActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <receiver android:name="com.froumobic.clean.junk.broadcast.UnloadBroadcast">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <action android:name="android.intent.action.PACKAGE_FULLY_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.froumobic.clean.junk.service.XuanfuService"
            android:excludeFromRecents="true" />
        <service
            android:name="com.froumobic.clean.junk.service.NotificationService"
            android:enabled="true"
            android:excludeFromRecents="true"
            android:exported="false" />

        <activity
            android:name="com.froumobic.clean.junk.mactivity.FileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.froumobic.clean.junk.mactivity.FileListActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.froumobic.clean.junk.mactivity.FileDocActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.froumobic.clean.junk.Zwidget.WidgetService"
            android:enabled="true" />

        <receiver
            android:name="com.froumobic.clean.junk.Zwidget.WidgetProvider"
            android:icon="@drawable/widget_4"
            android:label="@string/app_name_4_4">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <action android:name="app.froumobi.action.widget.ram" />
                <action android:name="app.froumobi.action.widget.update" />
            </intent-filter>
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/ram" />
        </receiver>

        <receiver
            android:name="com.froumobic.clean.junk.Zwidget.TempProvider"
            android:icon="@drawable/widget_temp_preview"
            android:label="@string/app_name_4_1">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <action android:name="app.froumobi.action.widget.temp" />
            </intent-filter>
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/temp" />
        </receiver>

        <service
            android:name="com.froumobic.clean.junk.Zwidget.TempService"
            android:enabled="true" />

    </application>

</manifest>