<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.myboost.junk">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <!--IMEI-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 读写SDCard权限 -->
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <!--获取最上层包名-->
    <uses-permission android:name="android.permission.GET_TASKS" />
    <!--可以访问使用量数据-->
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!--快捷方式-->
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />


    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <!--开机自启-->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <uses-permission
        android:name="android.permission.WRITE_MEDIA_STORAGE"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />


    <application
        android:name="com.myboost.junk.activityprivacy.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:replace="android:theme,android:supportsRtl,android:icon">
        <activity
            android:name="com.myboost.junk.activityprivacy.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"></activity>

        <activity
            android:name="com.myboost.junk.activityprivacy.LoadingActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.MEDIA_UNMOUNTED" />

                <data android:scheme="file" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.myboost.junk.activityprivacy.BatteriesActivityBoost"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostRubbishActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.RubbishAndRamActivityBoost"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostAllAppsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostPhonesMessageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.MemoryAvtivityBoost"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostSetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.SucceedActivityBoost"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostIgnoresAvtivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.IgnoresAddActivityBoost"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostDeepingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.NotifingAnimationActivityBoost"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostPermissingActivity"
            android:allowTaskReparenting="true"
            android:excludeFromRecents="true"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostNotifingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.NotifingSettingActivityBoost"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostSuspensionBallActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />
        <activity
            android:name="com.myboost.junk.activityprivacy.UnloadingResidualActivityBoost"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />

        <activity
            android:name="com.myboost.junk.activityprivacy.BoostPictActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.PictHuiActivityBoost"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostGoodGameActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.myboost.junk.activityprivacy.ShortCutingActivityBoost"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <receiver android:name="com.myboost.junk.privacybroadcasts.BoostUnloadResidualBroadcast">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.myboost.junk.privacyservices.SuspensionBallServiceBoost"
            android:excludeFromRecents="true" />
        <service
            android:name="com.myboost.junk.privacyservices.NotificationServiceBoost"
            android:enabled="true"
            android:excludeFromRecents="true" />

        <activity
            android:name="com.myboost.junk.activityprivacy.BoostFileManagerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.myboost.junk.activityprivacy.FileActivityBoost"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.myboost.junk.activityprivacy.BoostDocActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>