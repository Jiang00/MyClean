<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.icleaner.junk">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <!--IMEI-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 读写SDCard权限 -->
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <!--获取最上层包名-->
    <uses-permission android:name="android.permission.GET_TASKS" />
    <!--可以访问使用量数据-->
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!--快捷方式-->
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />


    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <!--开机自启-->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <uses-permission
        android:name="android.permission.WRITE_MEDIA_STORAGE"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />


    <application
        android:name="com.icleaner.junk.icleaneractivity.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:replace="android:theme,android:supportsRtl,android:icon">
        <activity
            android:name="com.icleaner.junk.icleaneractivity.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"></activity>

        <activity
            android:name="com.icleaner.junk.icleaneractivity.LoadingActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.MEDIA_UNMOUNTED" />

                <data android:scheme="file" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.icleaner.junk.icleaneractivity.BatteriesActivity"
            android:screenOrientation="portrait" />
        <!--垃圾-->
        <activity
            android:name="com.icleaner.junk.icleaneractivity.RubbishActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.icleaner.junk.RubbishActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.icleaner.junk.icleaneractivity.RubbishAndRamActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.AllAppsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.PhonesMessageActivity"
            android:screenOrientation="portrait" />
        <!--内存-->
        <activity
            android:name="com.icleaner.junk.icleaneractivity.MemoryAvtivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.icleaner.junk.MemoryAvtivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.icleaner.junk.icleaneractivity.SetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.SucceedActivity"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.IgnoresAvtivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.IgnoresAddActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.DeepingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.NotifingAnimationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.PermissingActivity"
            android:allowTaskReparenting="true"
            android:excludeFromRecents="true"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.MyNotifingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.MyNotifingSettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.SuspensionBallActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.UnloadingResidualActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />

        <activity
            android:name="com.icleaner.junk.icleaneractivity.PictActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.PictHuiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.GoodGameActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.icleaner.junk.icleaneractivity.ShortCutingActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <receiver android:name="com.icleaner.junk.mybroadcasts.UnloadResidualBroadcast">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.icleaner.junk.services.SuspensionBallService"
            android:excludeFromRecents="true" />
        <service
            android:name="com.icleaner.junk.services.MyNotificationService"
            android:enabled="true"
            android:excludeFromRecents="true" />

        <activity
            android:name="com.icleaner.junk.icleaneractivity.PhoneFileManagerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.icleaner.junk.icleaneractivity.PhoneFileActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.icleaner.junk.icleaneractivity.PhoneDocActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>