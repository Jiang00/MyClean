            android:gravity="center"
            android:orientation="vertical">

            <com.my.module.charge.saver.view.CustomerTypefaceTextView
                android:id="@+id/message_model"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sanxign"
                android:textColor="@color/A7"
                android:textSize="@dimen/s24"
                app:typeface="eos_clean.otf" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d12"
                android:orientation="horizontal">

                <com.my.module.charge.saver.view.CustomerTypefaceTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/message_title_1"
                    android:textColor="@color/A7"
                    android:textSize="@dimen/s16"
