                    android:layout_height="wrap_content"
                    android:text="Android"
                    android:textColor="@color/A7"
                    android:textSize="@dimen/s16"
                    app:typeface="eos_clean.otf" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

